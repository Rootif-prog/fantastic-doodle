-- Scripts:

local function OCPQ_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local Players = game:GetService("Players")
	
				-- Remote
				local Addwins = ReplicatedStorage.Points.Addwins -- RemoteEvent 
	
				-- Variables
				local HumanoidRootPart = Players.LocalPlayer.Character.HumanoidRootPart
	
				Addwins:FireServer(
					1,
					HumanoidRootPart,
					CFrame.new(-43, 5, 10, 1, 0, 0, 0, 1, 0, 0, 0, 1)
				)
			end)
			wait()
		end
	end)
end
coroutine.wrap(OCPQ_fake_script)()
local function IIEWBZ_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	-- TeleportButton.lua
	local TweenService = game:GetService("TweenService")
	local Players      = game:GetService("Players")
	
	local button = script.Parent               -- кнопка
	local frame  = button.Parent               -- её родитель, у которого меняем цвет/прозрачность
	
	-- Ховер-эффекты
	button.MouseEnter:Connect(function()
		TweenService:Create(frame, TweenInfo.new(1), {
			BackgroundTransparency = 0.5
		}):Play()
	end)
	
	button.MouseLeave:Connect(function()
		TweenService:Create(frame, TweenInfo.new(1), {
			BackgroundTransparency = 0.8
		}):Play()
	end)
	
	-- Клик: телепорт к рандомному игроку с визуальной отдачей
	button.MouseButton1Click:Connect(function()
		local success, err = pcall(function()
			-- Собираем остальных игроков с HumanoidRootPart
			local localPlayer = Players.LocalPlayer
			local others = {}
			for _, plr in ipairs(Players:GetPlayers()) do
				if plr ~= localPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
					table.insert(others, plr)
				end
			end
	
			if #others == 0 then
				error("Нет доступных игроков для телепорта")
			end
	
			-- Выбираем случайного и телепортируем
			local target = others[math.random(1, #others)]
			local targetHRP = target.Character.HumanoidRootPart
			local myChar = localPlayer.Character or localPlayer.CharacterAdded:Wait()
			local hrp = myChar:FindFirstChild("HumanoidRootPart")
			if not hrp then error("Не удалось найти HumanoidRootPart") end
	
			hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 5, 0)
		end)
	
		if not success then
			-- Ошибка: краснеем и возвращаем белый
			TweenService:Create(frame, TweenInfo.new(2), {
				BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)
			}):Play()
			wait()
			TweenService:Create(frame, TweenInfo.new(2), {
				BackgroundColor3 = Color3.new(1, 1, 1)
			}):Play()
			warn("Teleport error:", err)
		else
			-- Успех: зелёнеем и возвращаем белый
			TweenService:Create(frame, TweenInfo.new(2), {
				BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)
			}):Play()
			wait()
			TweenService:Create(frame, TweenInfo.new(2), {
				BackgroundColor3 = Color3.new(1, 1, 1)
			}):Play()
		end
	end)
	
end
coroutine.wrap(IIEWBZ_fake_script)()
local function ZCUP_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	local Players = game.Players; local IYMouse = game.Players.LocalPlayer:GetMouse()
	FLYING = false
	QEfly = true
	iyflyspeed = 1
	vehicleflyspeed = 1
	local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
	local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
	
	local flyKeyDown = IYMouse.KeyDown:Connect(function(KEY)
		vfly = false
		if KEY:lower() == 'w' then
			CONTROL.F = (vfly and vehicleflyspeed or iyflyspeed)
		elseif KEY:lower() == 's' then
			CONTROL.B = - (vfly and vehicleflyspeed or iyflyspeed)
		elseif KEY:lower() == 'a' then
			CONTROL.L = - (vfly and vehicleflyspeed or iyflyspeed)
		elseif KEY:lower() == 'd' then 
			CONTROL.R = (vfly and vehicleflyspeed or iyflyspeed)
		elseif QEfly and KEY:lower() == 'e' then
			CONTROL.Q = (vfly and vehicleflyspeed or iyflyspeed)*2
		elseif QEfly and KEY:lower() == 'q' then
			CONTROL.E = -(vfly and vehicleflyspeed or iyflyspeed)*2
		end
		pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Track end)
	end)
	local flyKeyUp = IYMouse.KeyUp:Connect(function(KEY)
		if KEY:lower() == 'w' then
			CONTROL.F = 0
		elseif KEY:lower() == 's' then
			CONTROL.B = 0
		elseif KEY:lower() == 'a' then
			CONTROL.L = 0
		elseif KEY:lower() == 'd' then
			CONTROL.R = 0
		elseif KEY:lower() == 'e' then
			CONTROL.Q = 0
		elseif KEY:lower() == 'q' then
			CONTROL.E = 0
		end
	end)
	function sFLY(vfly)
		vfly = false
		repeat wait() until Players.LocalPlayer and Players.LocalPlayer.Character and Players.LocalPlayer.Character.HumanoidRootPart and Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
		repeat wait() until IYMouse
		if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
	
		local T = Players.LocalPlayer.Character.HumanoidRootPart
		local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local SPEED = 0
	
		local function FLY()
			FLYING = true
			local BG = Instance.new('BodyGyro')
			local BV = Instance.new('BodyVelocity')
			BG.P = 9e4
			BG.Parent = T
			BV.Parent = T
			BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
			BG.cframe = T.CFrame
			BV.velocity = Vector3.new(0, 0, 0)
			BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
			task.spawn(function()
				repeat wait()
					iyflyspeed = _G.Stage
					if not vfly and Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
						Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = true
					end
					if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
						SPEED = 50
					elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
						SPEED = 0
					end
					if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
						BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (CONTROL.F + CONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
						lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
					elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
						BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (lCONTROL.F + lCONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
					else
						BV.velocity = Vector3.new(0, 0, 0)
					end
					BG.cframe = workspace.CurrentCamera.CoordinateFrame
				until not FLYING
				CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				SPEED = 0
				BG:Destroy()
				BV:Destroy()
				if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
					Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
				end
			end)
		end
		flyKeyDown = IYMouse.KeyDown:Connect(function(KEY)
			if KEY:lower() == 'w' then
				CONTROL.F = (vfly and vehicleflyspeed or iyflyspeed)
			elseif KEY:lower() == 's' then
				CONTROL.B = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif KEY:lower() == 'a' then
				CONTROL.L = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif KEY:lower() == 'd' then 
				CONTROL.R = (vfly and vehicleflyspeed or iyflyspeed)
			elseif QEfly and KEY:lower() == 'e' then
				CONTROL.Q = (vfly and vehicleflyspeed or iyflyspeed)*2
			elseif QEfly and KEY:lower() == 'q' then
				CONTROL.E = -(vfly and vehicleflyspeed or iyflyspeed)*2
			end
			pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Track end)
		end)
		flyKeyUp = IYMouse.KeyUp:Connect(function(KEY)
			if KEY:lower() == 'w' then
				CONTROL.F = 0
			elseif KEY:lower() == 's' then
				CONTROL.B = 0
			elseif KEY:lower() == 'a' then
				CONTROL.L = 0
			elseif KEY:lower() == 'd' then
				CONTROL.R = 0
			elseif KEY:lower() == 'e' then
				CONTROL.Q = 0
			elseif KEY:lower() == 'q' then
				CONTROL.E = 0
			end
		end)
		FLY()
	end
	
	function NOFLY()
		FLYING = false
		if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
		if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
			Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
		end
		pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Custom end)
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
	
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
	
		if status then
			NOFLY()
			wait()
			sFLY()
		else
			NOFLY()
		end
	end)
end
coroutine.wrap(ZCUP_fake_script)()
local function JKMBN_fake_script() -- TextBox.Controller 
	local script = Instance.new('LocalScript', TextBox)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.Focused:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.439216, 0.439216, 0.439216)}):Play()
		end)
	end)
	
	script.Parent.FocusLost:Connect(function()
		local sucess, err = pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			local textbox = script.Parent
			local text = textbox.Text
	
			local tonum = tonumber(text)
	
			if tonum ~= nil then
				_G.Stage = tonum
			else
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.698039, 0.0705882, 0.164706)}):Play()
				wait()
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			end
		end)
	
		if not sucess then print(err) end
	end)
end
coroutine.wrap(JKMBN_fake_script)()
local function UXKMLWG_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService('ReplicatedStorage')
				local Players = game:GetService('Players')
	
				-- Remote
				local WeaponReloadRequest =
					ReplicatedStorage.WeaponsSystem.Network.WeaponReloadRequest -- RemoteEvent
	
				-- Variables
				local SMG = Players.LocalPlayer.Character.SMG
	
				WeaponReloadRequest:FireServer(SMG)
	
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local Players = game:GetService("Players")
	
				-- Remote
				local WeaponReloadRequest = ReplicatedStorage.WeaponsSystem.Network.WeaponReloadRequest -- RemoteEvent 
	
				-- Variables
				local Pistol = Players.LocalPlayer.Backpack.Pistol
	
				WeaponReloadRequest:FireServer(
					Pistol
				)
	
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local Players = game:GetService("Players")
	
				-- Remote
				local WeaponReloadRequest = ReplicatedStorage.WeaponsSystem.Network.WeaponReloadRequest -- RemoteEvent 
	
				-- Variables
				local Crossbow = Players.LocalPlayer.Backpack.Crossbow
	
				WeaponReloadRequest:FireServer(
					Crossbow
				)
			end)
			wait()
		end
	end)
end
