	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local A_1 = true
				local Event = game:GetService("ReplicatedStorage").Remotes.FightEnded
				Event:FireServer(A_1)
				local A_1 = true
				local Event = game:GetService("ReplicatedStorage").Remotes.FightEnded
				Event:FireServer(A_1)
				local A_1 = true
				local Event = game:GetService("ReplicatedStorage").Remotes.FightEnded
				Event:FireServer(A_1)
				local A_1 = true
				local Event = game:GetService("ReplicatedStorage").Remotes.FightEnded
				Event:FireServer(A_1)
				local A_1 = true
				local Event = game:GetService("ReplicatedStorage").Remotes.FightEnded
				Event:FireServer(A_1)
			end)
			wait()
		end
	end)
end
