				Event:FireServer(A_1)
				local A_1 = "EyeShape"
				local Event = game:GetService("ReplicatedStorage").Remotes.ClickForPoints
				Event:FireServer(A_1)
				local A_1 = "EyeShape"
				local Event = game:GetService("ReplicatedStorage").Remotes.ClickForPoints
				Event:FireServer(A_1)
				local A_1 = "EyeShape"
				local Event = game:GetService("ReplicatedStorage").Remotes.ClickForPoints
