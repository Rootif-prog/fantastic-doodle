-- Scripts:

local function ZOGAC_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
	
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
	
		while status do
			pcall(function()
				local player = game:GetService("Players").LocalPlayer
				local tycoonObject = player:FindFirstChild("Tycoon")
	
				-- –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ ObjectValue Tycoon —É –∏–≥—Ä–æ–∫–∞
				if not tycoonObject or not tycoonObject:IsA("ObjectValue") then
					warn("‚ö†Ô∏è –ù–µ –Ω–∞–π–¥–µ–Ω ObjectValue 'Tycoon' —É –∏–≥—Ä–æ–∫–∞!")
					return
				end
	
				-- –ü–æ–ª—É—á–∞–µ–º —Å—Å—ã–ª–∫—É –Ω–∞ —Å–∞–º —Ç–∏–∫—É–Ω (Model)
				local tycoonModel = tycoonObject.Value
				if not tycoonModel then
					warn("‚ö†Ô∏è Tycoon.Value –Ω–µ —Å–æ–¥–µ—Ä–∂–∏—Ç —Å—Å—ã–ª–∫–∏ –Ω–∞ –º–æ–¥–µ–ª—å!")
					return
				end
	
				-- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å—Ç—Ä—É–∫—Ç—É—Ä—É —Ç–∏–∫—É–Ω–∞
				local essentials = tycoonModel:FindFirstChild("Essentials")
				local dropsFolder = tycoonModel:FindFirstChild("Drops")
				local collectingArea = essentials and essentials:FindFirstChild("PartCollectingArea")
	
				if not essentials then warn("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω 'Essentials' –≤ —Ç–∏–∫—É–Ω–µ!") return end
				if not dropsFolder then warn("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω–∞ –ø–∞–ø–∫–∞ 'Drops' –≤ —Ç–∏–∫—É–Ω–µ!") return end
				if not collectingArea then warn("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω–∞ 'PartCollectingArea'!") return end
	
				-- –ù–∞—Ö–æ–¥–∏–º 4-–π —ç–ª–µ–º–µ–Ω—Ç –≤ –∑–æ–Ω–µ —Å–±–æ—Ä–∞
				local targetPart = collectingArea:GetChildren()[4]
				if not targetPart or not targetPart:FindFirstChildOfClass("TouchTransmitter") then
					warn("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω TouchInterest –≤ –∑–æ–Ω–µ —Å–±–æ—Ä–∞!")
					return
				end
	
				-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
				local DROP_HEIGHT = 1
				local RANDOM_SPREAD = 3
	
				-- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –≤—Å–µ—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
				for _, item in ipairs(dropsFolder:GetChildren()) do
					if item:IsA("BasePart") then
						-- –í—ã—á–∏—Å–ª—è–µ–º –ø–æ–∑–∏—Ü–∏—é —Å–æ —Å–ª—É—á–∞–π–Ω—ã–º —Å–º–µ—â–µ–Ω–∏–µ–º
						local dropPos = targetPart.Position + Vector3.new(
							(math.random() - 0.5) * 2 * RANDOM_SPREAD,
							DROP_HEIGHT,
							(math.random() - 0.5) * 2 * RANDOM_SPREAD
						)
	
						-- –ü—Ä–∏–º–µ–Ω—è–µ–º —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—é —Å —Ñ–∏–∑–∏–∫–æ–π
						item.CFrame = CFrame.new(dropPos)
						item.Anchored = false
						item.CanCollide = true
						item.AssemblyLinearVelocity = Vector3.new(0, -5, 0) -- –ü–∞–¥–µ–Ω–∏–µ –≤–Ω–∏–∑
						item:ApplyAngularImpulse(Vector3.new(
							(math.random() - 0.5) * 20, -- –í—Ä–∞—â–µ–Ω–∏–µ X
							(math.random() - 0.5) * 20, -- –í—Ä–∞—â–µ–Ω–∏–µ Y
							(math.random() - 0.5) * 20  -- –í—Ä–∞—â–µ–Ω–∏–µ Z
							))
					end
				end
	
				print("‚úÖ –£—Å–ø–µ—à–Ω–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω—ã –ø—Ä–µ–¥–º–µ—Ç—ã –≤ —Ç–∏–∫—É–Ω–µ "..tycoonModel.Name)
			end)
			wait()
		end
	end)
end
coroutine.wrap(ZOGAC_fake_script)()
local function YDUGW_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local player = game:GetService("Players").LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
				local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	
				-- –°–æ—Ö—Ä–∞–Ω—è–µ–º –∏—Å—Ö–æ–¥–Ω—É—é –ø–æ–∑–∏—Ü–∏—é
				local originalPosition = humanoidRootPart.Position
				local originalCFrame = humanoidRootPart.CFrame
	
				-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –æ–±—ä–µ–∫—Ç–∞ –ø–æ –ø—É—Ç–∏
				local function findObject(path)
					local current = workspace
					for _, partName in ipairs(path) do
						current = current:FindFirstChild(partName)
						if not current then return nil end
					end
					return current
				end
	
				-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ç–∏–∫–æ–Ω–∞ –∏–≥—Ä–æ–∫–∞
				local function getPlayerTycoon()
					local tycoonObject = player:FindFirstChild("Tycoon")
					if not tycoonObject or not tycoonObject:IsA("ObjectValue") then
						warn("Tycoon –Ω–µ –Ω–∞–π–¥–µ–Ω —É –∏–≥—Ä–æ–∫–∞!")
						return nil
					end
					return tycoonObject.Value
				end
	
				-- –ü–æ–ª—É—á–∞–µ–º —Ç–∏–∫–æ–Ω –∏–≥—Ä–æ–∫–∞
				local playerTycoon = getPlayerTycoon()
				if not playerTycoon then return end
	
				-- –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ü–≤–µ—Ç –∫–æ–º–∞–Ω–¥—ã (–Ω–∞–∑–≤–∞–Ω–∏–µ —Ç–∏–∫–æ–Ω–∞)
				local tycoonName = playerTycoon.Name
				print("–ò–≥—Ä–æ–∫ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –≤ —Ç–∏–∫–æ–Ω–µ:", tycoonName)
	
				-- –ü—É—Ç—å –∫ —Ü–µ–ª–µ–≤–æ–º—É –æ–±—ä–µ–∫—Ç—É (—Å –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–º –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ–º —Ü–≤–µ—Ç–∞)
				local targetPath = {"TycoonKit", "Tycoons", tycoonName, "Essentials", "CurrencyCollector", "Giver", "TouchInterest"}
				local targetPart = findObject(targetPath)
	
				if targetPart then
					-- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ü–µ–ª–µ–≤–æ–º—É –æ–±—ä–µ–∫—Ç—É
					humanoidRootPart.CFrame = targetPart.Parent.CFrame + Vector3.new(0, 3, 0) -- –°–º–µ—â–µ–Ω–∏–µ –ø–æ –≤—ã—Å–æ—Ç–µ
	
					-- –ñ–¥–µ–º 2 —Å–µ–∫—É–Ω–¥—ã
					wait(0.5)
	
					-- –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –Ω–∞ –∏—Å—Ö–æ–¥–Ω—É—é –ø–æ–∑–∏—Ü–∏—é
					humanoidRootPart.CFrame = originalCFrame
				else
					warn("–ù–µ —É–¥–∞–ª–æ—Å—å –Ω–∞–π—Ç–∏ —Ü–µ–ª–µ–≤–æ–π –æ–±—ä–µ–∫—Ç –ø–æ –ø—É—Ç–∏: TycoonKit.Tycoons." .. tycoonName .. ".Essentials.CurrencyCollector.Giver.TouchInterest")
				end
			end)
			wait()
		end
	end)
end
coroutine.wrap(YDUGW_fake_script)()
local function IOXH_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local player = game:GetService("Players").LocalPlayer
	
				local function getPlayerTycoon()
					local tycoonObject = player:FindFirstChild("Tycoon")
					if not tycoonObject or not tycoonObject:IsA("ObjectValue") then
						warn("Tycoon –Ω–µ –Ω–∞–π–¥–µ–Ω —É –∏–≥—Ä–æ–∫–∞!")
						return nil
					end
					return tycoonObject.Value
				end
	
				local function findPrompt(tycoon)
					for _, desc in ipairs(tycoon:GetDescendants()) do
						if desc:IsA("ProximityPrompt") and desc.Name == "CoalProxPrompt" then
							return desc
						end
					end
					return nil
				end
	
				local function activatePromptOnce()
					local tycoon = getPlayerTycoon()
					if not tycoon then return end
	
					local prompt = findPrompt(tycoon)
					if not prompt then
						warn("–ü—Ä–æ–º–ø—Ç 'CoalProxPrompt' –Ω–µ –Ω–∞–π–¥–µ–Ω!")
						return
					end
	
					if prompt.Enabled then
						prompt:InputHoldBegin()
						task.wait(prompt.HoldDuration)
						prompt:InputHoldEnd()
						print("–ü—Ä–æ–º–ø—Ç –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω –æ–¥–∏–Ω —Ä–∞–∑.")
					else
						warn("–ü—Ä–æ–º–ø—Ç –Ω–µ –∞–∫—Ç–∏–≤–µ–Ω.")
					end
				end
	
				activatePromptOnce()
			end)
			wait()
		end
	end)
end
coroutine.wrap(IOXH_fake_script)()
local function YDIERQ_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	
		-- –§—É–Ω–∫—Ü–∏—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ç–∏–∫–æ–Ω–∞ –∏–≥—Ä–æ–∫–∞ —Å —É–ª—É—á—à–µ–Ω–Ω–æ–π –æ–±—Ä–∞–±–æ—Ç–∫–æ–π –æ—à–∏–±–æ–∫
		local function getPlayerTycoon()
			local tycoonObject = player:FindFirstChild("Tycoon")
			return (tycoonObject and tycoonObject:IsA("ObjectValue")) and tycoonObject.Value or nil
		end
	
		-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
		local function teleportToDropper()
			-- –ü–æ–ª—É—á–∞–µ–º —Ç–µ–∫—É—â–∏–π —Ç–∏–∫–æ–Ω
			local playerTycoon = getPlayerTycoon()
			if not playerTycoon then
				warn("‚ùå –ò–≥—Ä–æ–∫ –Ω–µ –∏–º–µ–µ—Ç —Ç–∏–∫–æ–Ω–∞ –∏–ª–∏ –æ–Ω –Ω–µ –Ω–∞—Å—Ç—Ä–æ–µ–Ω –ø—Ä–∞–≤–∏–ª—å–Ω–æ")
				return false
			end
	
			local tycoonName = playerTycoon.Name
			print("üîç –û–±–Ω–∞—Ä—É–∂–µ–Ω —Ç–∏–∫–æ–Ω –∏–≥—Ä–æ–∫–∞:", tycoonName)
	
			-- –°—Ç—Ä–æ–∏–º –ø—É—Ç—å –∫ —Ü–µ–ª–µ–≤–æ–π —Ç–æ—á–∫–µ
			local targetPath = {"TycoonKit", "Tycoons", tycoonName, "Purchased", "ManualDropper", "Dropper", "Base"}
			local current = workspace
	
			-- –ü–æ–∏—Å–∫ –æ–±—ä–µ–∫—Ç–∞ —Å –ø—Ä–æ–≤–µ—Ä–∫–æ–π –∫–∞–∂–¥–æ–π —á–∞—Å—Ç–∏ –ø—É—Ç–∏
			for _, partName in ipairs(targetPath) do
				current = current:FindFirstChild(partName)
				if not current then
					warn("‚ö†Ô∏è –ù–µ –Ω–∞–π–¥–µ–Ω–∞ —á–∞—Å—Ç—å –ø—É—Ç–∏: " .. partName)
					return false
				end
			end
	
			-- –ü–æ–ª—É—á–∞–µ–º 3-–π –¥–æ—á–µ—Ä–Ω–∏–π —ç–ª–µ–º–µ–Ω—Ç Base
			local targetChildren = current:GetChildren()
			if #targetChildren < 3 then
				warn("‚ö†Ô∏è –í Base –º–µ–Ω—å—à–µ 3 –¥–æ—á–µ—Ä–Ω–∏—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤")
				return false
			end
	
			local targetPart = targetChildren[3]
			if not targetPart then
				warn("‚ö†Ô∏è 3-–π –¥–æ—á–µ—Ä–Ω–∏–π —ç–ª–µ–º–µ–Ω—Ç –Ω–µ –Ω–∞–π–¥–µ–Ω")
				return false
			end
	
			-- –†–∞—Å—Å—á–∏—Ç—ã–≤–∞–µ–º –±–µ–∑–æ–ø–∞—Å–Ω—É—é –ø–æ–∑–∏—Ü–∏—é
			local safeOffset = Vector3.new(5, 3, -5) -- Y: +3, Z: -2
			local targetCFrame = targetPart.CFrame + safeOffset
	
			-- –í—ã–ø–æ–ª–Ω—è–µ–º —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—é
			humanoidRootPart.CFrame = targetCFrame
			print(string.format("‚úÖ –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∞! –¢–∏–∫–æ–Ω: %s | –ü–æ–∑–∏—Ü–∏—è: %s", 
				tycoonName, tostring(targetCFrame.Position)))
			return true
		end
	
		-- –ó–∞–ø—É—Å–∫–∞–µ–º —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—é
		teleportToDropper()
	
	end)
	if not sucess then
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
	else
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
	end
end
