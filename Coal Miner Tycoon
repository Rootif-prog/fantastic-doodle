					local tycoonObject = player:FindFirstChild("Tycoon")
					if not tycoonObject or not tycoonObject:IsA("ObjectValue") then
						warn("Tycoon не найден у игрока!")
						return nil
					end
					return tycoonObject.Value
				end
	
				local function findPrompt(tycoon)
					for _, desc in ipairs(tycoon:GetDescendants()) do
						if desc:IsA("ProximityPrompt") and desc.Name == "CoalProxPrompt" then
							return desc
						end
					end
					return nil
				end
	
				local function activatePromptOnce()
					local tycoon = getPlayerTycoon()
					if not tycoon then return end
	
					local prompt = findPrompt(tycoon)
					if not prompt then
						warn("Промпт 'CoalProxPrompt' не найден!")
						return
					end
	
					if prompt.Enabled then
						prompt:InputHoldBegin()
						task.wait(prompt.HoldDuration)
						prompt:InputHoldEnd()
						print("Промпт активирован один раз.")
					else
						warn("Промпт не активен.")
					end
				end
	
				activatePromptOnce()
			end)
			wait()
		end
	end)
end
coroutine.wrap(IOXH_fake_script)()
local function YDIERQ_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	
		-- Функция получения тикона игрока с улучшенной обработкой ошибок
		local function getPlayerTycoon()
			local tycoonObject = player:FindFirstChild("Tycoon")
			return (tycoonObject and tycoonObject:IsA("ObjectValue")) and tycoonObject.Value or nil
		end
	
		-- Основная функция телепортации
		local function teleportToDropper()
			-- Получаем текущий тикон
			local playerTycoon = getPlayerTycoon()
			if not playerTycoon then
				warn("❌ Игрок не имеет тикона или он не настроен правильно")
				return false
			end
	
			local tycoonName = playerTycoon.Name
			print("🔍 Обнаружен тикон игрока:", tycoonName)
	
			-- Строим путь к целевой точке
			local targetPath = {"TycoonKit", "Tycoons", tycoonName, "Purchased", "ManualDropper", "Dropper", "Base"}
			local current = workspace
	
			-- Поиск объекта с проверкой каждой части пути
			for _, partName in ipairs(targetPath) do
				current = current:FindFirstChild(partName)
				if not current then
					warn("⚠️ Не найдена часть пути: " .. partName)
					return false
				end
			end
	
			-- Получаем 3-й дочерний элемент Base
			local targetChildren = current:GetChildren()
			if #targetChildren < 3 then
				warn("⚠️ В Base меньше 3 дочерних элементов")
				return false
			end
	
			local targetPart = targetChildren[3]
			if not targetPart then
				warn("⚠️ 3-й дочерний элемент не найден")
				return false
			end
	
			-- Рассчитываем безопасную позицию
			local safeOffset = Vector3.new(5, 3, -5) -- Y: +3, Z: -2
			local targetCFrame = targetPart.CFrame + safeOffset
	
			-- Выполняем телепортацию
			humanoidRootPart.CFrame = targetCFrame
			print(string.format("✅ Телепортация выполнена! Тикон: %s | Позиция: %s", 
				tycoonName, tostring(targetCFrame.Position)))
			return true
		end
	
		-- Запускаем телепортацию
		teleportToDropper()
	
	end)
	if not sucess then
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
	else
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
	end
end
coroutine.wrap(YDIERQ_fake_script)()
local function QAGX_fake_script() -- Trigger_5.Controller 
	local script = Instance.new('LocalScript', Trigger_5)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local player = game:GetService("Players").LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	
		-- Функция для получения тикона игрока
		local function getPlayerTycoon()
			local tycoonObject = player:FindFirstChild("Tycoon")
			if not tycoonObject or not tycoonObject:IsA("ObjectValue") then
				warn("Tycoon не найден у игрока!")
				return nil
			end
			return tycoonObject.Value
		end
	
		-- Получаем тикон игрока
		local playerTycoon = getPlayerTycoon()
		if not playerTycoon then return end
	
		-- Определяем цвет команды (название тикона)
		local tycoonName = playerTycoon.Name
		print("Игрок находится в тиконе:", tycoonName)
	
		-- Путь к TycoonSpawn
		local targetPath = {"TycoonKit", "Tycoons", tycoonName, "Essentials", "TycoonSpawn"}
		local current = workspace
	
		-- Поиск объекта по пути
		for _, partName in ipairs(targetPath) do
			current = current:FindFirstChild(partName)
			if not current then
				warn("Не удалось найти часть пути: " .. partName)
				return
			end
		end
	
		-- Телепортация к TycoonSpawn с небольшим смещением по высоте (+3)
		local targetCFrame = current.CFrame + Vector3.new(0, 3, 0)
		humanoidRootPart.CFrame = targetCFrame
	
		print("Телепортация к TycoonSpawn выполнена (смещение Y = +3)!")
	end)
	if not sucess then
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
	else
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
	end
end
