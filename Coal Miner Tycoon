-- Scripts:

local function ZOGAC_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
	
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
	
		while status do
			pcall(function()
				local player = game:GetService("Players").LocalPlayer
				local tycoonObject = player:FindFirstChild("Tycoon")
	
				-- Проверяем наличие ObjectValue Tycoon у игрока
				if not tycoonObject or not tycoonObject:IsA("ObjectValue") then
					warn("⚠️ Не найден ObjectValue 'Tycoon' у игрока!")
					return
				end
	
				-- Получаем ссылку на сам тикун (Model)
				local tycoonModel = tycoonObject.Value
				if not tycoonModel then
					warn("⚠️ Tycoon.Value не содержит ссылки на модель!")
					return
				end
	
				-- Проверяем структуру тикуна
				local essentials = tycoonModel:FindFirstChild("Essentials")
				local dropsFolder = tycoonModel:FindFirstChild("Drops")
				local collectingArea = essentials and essentials:FindFirstChild("PartCollectingArea")
	
				if not essentials then warn("❌ Не найден 'Essentials' в тикуне!") return end
				if not dropsFolder then warn("❌ Не найдена папка 'Drops' в тикуне!") return end
				if not collectingArea then warn("❌ Не найдена 'PartCollectingArea'!") return end
	
				-- Находим 4-й элемент в зоне сбора
				local targetPart = collectingArea:GetChildren()[4]
				if not targetPart or not targetPart:FindFirstChildOfClass("TouchTransmitter") then
					warn("❌ Не найден TouchInterest в зоне сбора!")
					return
				end
	
				-- Настройки телепортации
				local DROP_HEIGHT = 1
				local RANDOM_SPREAD = 3
	
				-- Телепортация всех предметов
				for _, item in ipairs(dropsFolder:GetChildren()) do
					if item:IsA("BasePart") then
						-- Вычисляем позицию со случайным смещением
						local dropPos = targetPart.Position + Vector3.new(
							(math.random() - 0.5) * 2 * RANDOM_SPREAD,
							DROP_HEIGHT,
							(math.random() - 0.5) * 2 * RANDOM_SPREAD
						)
	
						-- Применяем телепортацию с физикой
						item.CFrame = CFrame.new(dropPos)
						item.Anchored = false
						item.CanCollide = true
						item.AssemblyLinearVelocity = Vector3.new(0, -5, 0) -- Падение вниз
						item:ApplyAngularImpulse(Vector3.new(
							(math.random() - 0.5) * 20, -- Вращение X
							(math.random() - 0.5) * 20, -- Вращение Y
							(math.random() - 0.5) * 20  -- Вращение Z
							))
					end
				end
	
				print("✅ Успешно телепортированы предметы в тикуне "..tycoonModel.Name)
			end)
			wait()
		end
	end)
end
coroutine.wrap(ZOGAC_fake_script)()
local function YDUGW_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local player = game:GetService("Players").LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
				local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	
				-- Сохраняем исходную позицию
				local originalPosition = humanoidRootPart.Position
				local originalCFrame = humanoidRootPart.CFrame
	
				-- Функция для поиска объекта по пути
				local function findObject(path)
					local current = workspace
					for _, partName in ipairs(path) do
						current = current:FindFirstChild(partName)
						if not current then return nil end
					end
					return current
				end
	
				-- Функция для получения тикона игрока
				local function getPlayerTycoon()
					local tycoonObject = player:FindFirstChild("Tycoon")
					if not tycoonObject or not tycoonObject:IsA("ObjectValue") then
						warn("Tycoon не найден у игрока!")
						return nil
					end
					return tycoonObject.Value
				end
	
				-- Получаем тикон игрока
				local playerTycoon = getPlayerTycoon()
				if not playerTycoon then return end
	
				-- Определяем цвет команды (название тикона)
				local tycoonName = playerTycoon.Name
				print("Игрок находится в тиконе:", tycoonName)
	
				-- Путь к целевому объекту (с автоматическим определением цвета)
				local targetPath = {"TycoonKit", "Tycoons", tycoonName, "Essentials", "CurrencyCollector", "Giver", "TouchInterest"}
				local targetPart = findObject(targetPath)
	
				if targetPart then
					-- Телепортация к целевому объекту
					humanoidRootPart.CFrame = targetPart.Parent.CFrame + Vector3.new(0, 3, 0) -- Смещение по высоте
	
					-- Ждем 2 секунды
					wait(0.5)
	
					-- Возвращаемся на исходную позицию
					humanoidRootPart.CFrame = originalCFrame
				else
					warn("Не удалось найти целевой объект по пути: TycoonKit.Tycoons." .. tycoonName .. ".Essentials.CurrencyCollector.Giver.TouchInterest")
				end
			end)
			wait()
		end
	end)
end
coroutine.wrap(YDUGW_fake_script)()
