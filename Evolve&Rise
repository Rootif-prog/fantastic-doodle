-- Scripts:

local function ETJUS_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local AURA1 = ReplicatedStorage.GameManager.RemoteManager["2001"] -- RemoteEvent 
				AURA1:FireServer(
					{
						skillId = 1
					}
				)
			end)
			wait()
		end
	end)
end
coroutine.wrap(ETJUS_fake_script)()
local function BLPMDI_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService('ReplicatedStorage')
				local Phonk1 = ReplicatedStorage.GameManager.RemoteManager['20017'] -- RemoteEvent
				Phonk1:FireServer(10015, 3)
			end)
			wait()
		end
	end)
end
coroutine.wrap(BLPMDI_fake_script)()
local function ZWVHVZ_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x = TweenInfo.new(0.7)
	local status = false
	local defaultWalkSpeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed  -- Стандартная скорость
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			-- Включаем увеличение скорости
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0, 24, 0, 0)}):Play()
	
			-- Увеличиваем скорость до 50
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
			status = true
	
			-- Цикл, который поддерживает скорость 50
			spawn(function()
				while status do
					wait(0.1)  -- Проверяем каждые 0.1 секунды
					if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= 50 then
						game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
					end
				end
			end)
	
		else
			-- Выключаем увеличение скорости
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0, 0, 0, 0)}):Play()
	
			-- Возвращаем стандартную скорость
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = defaultWalkSpeed
			status = false
		end
	end)
	
end
coroutine.wrap(ZWVHVZ_fake_script)()
local function YIGWR_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.8}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err = pcall(function()
			-- LocalScript (например, в StarterPlayerScripts)
	
			local Players = game:GetService("Players")
			local player  = Players.LocalPlayer
	
			-- Целевая CFrame
			local targetCFrame = CFrame.new(
				171.462387, 0.754895687, -1259.3623,
				-0.115598917, 0, 0.993295968,
				0,           1, 0,
				-0.993295968, 0, -0.115598917
			)
	
			-- Функция телепорта
			local function teleportCharacter(char)
				local hrp = char:FindFirstChild("HumanoidRootPart")
				if hrp then
					hrp.CFrame = targetCFrame
				end
			end
	
			-- Телепорт сразу, если персонаж уже загружен
			if player.Character then
				teleportCharacter(player.Character)
			end
	
			-- И при каждом респавне
			player.CharacterAdded:Connect(teleportCharacter)
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()
		end
	end)
	
end
