local function YFLRHJU_fake_script() -- Toggle.Controller 
	local script = Instance.new('LocalScript', Toggle)

	local toggle : ImageButton = script.Parent
	local ts :TweenService = game:GetService("TweenService")
	local info : TweenInfo = TweenInfo.new(1.25,Enum.EasingStyle.Cubic)
	
	local status : boolean = false -- true: Close; false: Open
	local isalreadydoing : boolean = false	
	
	local pc = {Size = toggle.Parent.Size}; local po = {Size = toggle.Parent.Parent.Size}
	print(po.Size)
	--[[
			if not status then
				isalreadydoing = true
				status = true
				ts:Create(script.Parent,TweenInfo.new(1,Enum.EasingStyle.Cubic),{Rotation =180}):Play()
				local tween = ts:Create(closingtab,info,{Size = UDim2.new(0,311,0,282)})
				local x = ts:Create(closingtab.Container,TweenInfo.new(0.5),{BackgroundTransparency = 1})
				closingtab.Container.ContainerList.Visible = false
				tween:Play()
				x:Play()
				x.Completed:Wait()
				tween.Completed:Wait()
				isalreadydoing = false
			else
				status = false
				isalreadydoing = true
				local tween = ts:Create(closingtab,info,{Size = script.Parent.Parent.Size})
				ts:Create(script.Parent,TweenInfo.new(1,Enum.EasingStyle.Cubic),{Rotation =0}):Play()
				tween:Play()
				tween.Completed:Wait()
				ts:Create(closingtab.Container,TweenInfo.new(0.5),{BackgroundTransparency = 0.5}):Play()
				isalreadydoing = false
				closingtab.Container.ContainerList.Visible = true
			end
	]]
	
	toggle.MouseButton1Click:Connect(function()
		if isalreadydoing then return end
		local success, err = pcall(function()
			local closingtab = script.Parent.Parent.Parent
			
			if not status then
				status = true; isalreadydoing = true
				local t = ts:Create(closingtab,info,pc)
				t:Play()
				local t2 = ts:Create(closingtab.Container,info,{BackgroundTransparency = 1})
				t2:Play()
				ts:Create(toggle,info,{Rotation = 180}):Play()
				closingtab.Container.ContainerList.Visible = false
				
				isalreadydoing = false; 
			else
				status = false; isalreadydoing = true
				local t = ts:Create(closingtab,info,po)
				t:Play()
				ts:Create(toggle,info,{Rotation = 0}):Play()
				t.Completed:Wait()
				local t2 = ts:Create(closingtab.Container,info,{BackgroundTransparency = 0.9})
				t2:Play()
				closingtab.Container.ContainerList.Visible = true
				
				isalreadydoing = false;
			end
		end)
		
		if not success then print(err) end
	end)
end
