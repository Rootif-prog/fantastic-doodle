-- Scripts:

local function PLAKPOM_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local ProcessReturn = ReplicatedStorage.Events.ProcessReturn -- RemoteFunction 
				local RemoteArgs = {
					true,
					[3] = 2724
				}
				ProcessReturn:InvokeServer(unpack(RemoteArgs, 1, table.maxn(RemoteArgs)))
			end)
			wait()
		end
	end)
end
coroutine.wrap(PLAKPOM_fake_script)()
local function YHMVE_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				-- Скрипт для экзекьютора: массовое открытие яиц по имени и радиусу вокруг игрока
	
				local Players = game:GetService("Players")
				local ReplicatedStorage = game:GetService('ReplicatedStorage')
	
				-- Получаем локального игрока и его HumanoidRootPart
				local player      = Players.LocalPlayer
				local character   = player.Character or player.CharacterAdded:Wait()
				local hrp         = character:WaitForChild("HumanoidRootPart")
	
				-- RemoteFunction для открытия яиц
				local EggHatch = ReplicatedStorage.Events.EggHatch -- RemoteFunction
	
				-- Папка с яйцами
				local eggFolder = workspace:WaitForChild("EggShop")
	
				-- Настройка радиуса магнитуды (в студиях Roblox единица = 1 метр)
				local radius = 30 -- выбранный радиус для покрытия яиц из скриншота
	
				-- Функция открытия одного яйца
				local function hatchEgg(eggModel)
					EggHatch:InvokeServer(eggModel, 1)
				end
	
				-- Основной цикл: перебираем потомков в EggShop и открываем только яйца с именем от "1" до "16" в указанном радиусе
				for _, egg in ipairs(eggFolder:GetChildren()) do
					local nameNum = tonumber(egg.Name)
					if nameNum and nameNum >= 1 and nameNum <= 16 then
						-- Определяем центральный BasePart модели
						local part = egg.PrimaryPart or egg:FindFirstChildWhichIsA("BasePart")
						if part then
							local distance = (part.Position - hrp.Position).Magnitude
							if distance <= radius then
								hatchEgg(egg)
								wait() -- задержка между запросами
							end
						else
							warn("У модели", egg.Name, "нет BasePart для определения позиции.")
						end
					end
				end
	
				print("Открытие яиц по имени 1-16 в радиусе", radius, "студий завершено.")
			end)
			wait()
		end
	end)
end
coroutine.wrap(YHMVE_fake_script)()
local function FZABKEI_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.8}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err = pcall(function()
			local Players = game:GetService("Players")
			local player  = Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local hrp = character:WaitForChild("HumanoidRootPart")
			local swimFolder = workspace:WaitForChild("Swim"):WaitForChild("1")
			local swimChildren = swimFolder:GetChildren()
			local target = swimChildren[14]
			if target and target:IsA("BasePart") then
				hrp.CFrame = target.CFrame
			else
				warn("Не удалось найти 15-й BasePart в workspace.Swim['1']!")
			end
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()
		end
	end)
	
end
