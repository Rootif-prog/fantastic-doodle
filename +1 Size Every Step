local function KPUHW_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				-- Простой скрипт: телепорт на два заданных CFrame (с задержкой между ними)
	
				local Players = game:GetService("Players")
				local player  = Players.LocalPlayer
				local root    = player.Character:WaitForChild("HumanoidRootPart")
	
				-- Первое место телепорта
				local TP1 = CFrame.new(
					131.837906, 466.159576, -10.4474125,
					0.646039188, 0,          0.763304353,
					0,          1,          0,
					-0.763304353, 0,          0.646039188
				)
	
				-- Второе место телепорта
				local TP2 = CFrame.new(
					99.4247971, 1.99864328, -160.230576,
					-1,         0,           0,
					0,         1,           0,
					0,         0,          -1
				)
	
				-- Телепортируемся первым
				root.CFrame = TP1
	
				-- Делаем паузу (например, 1 секунда) — при необходимости подстройте время
				wait(0.5)
	
				-- Телепортируемся вторым
				root.CFrame = TP2
			end)
			wait()
		end
	end)
end
