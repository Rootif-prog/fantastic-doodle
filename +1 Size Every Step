-- Scripts:

local function UHJJMK_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	-- Кнопка: включить бесконечный “флай” вперёд–назад, при отключении телепорт на RETURN_CFRAME
	
	local Ts       = game:GetService("TweenService")
	local RunSvc   = game:GetService("RunService")
	local Info     = TweenInfo.new(1.3)
	local XInfo    = TweenInfo.new(0.7)
	local status   = false
	
	-- Параметры полёта
	local MOVE_SPEED     = 200    -- studs/сек
	local CYCLE_DURATION = 2      -- полный цикл вперёд+назад (сек)
	local HALF_CYCLE     = CYCLE_DURATION / 2
	
	-- Точка старта полёта
	local START_CFRAME = CFrame.new(
		-561.827759, 683.490051, -160.690552,
		-0.999939501, -0.00254763686, 0.0106979469,
		-1.21327925e-08, 0.972796082, 0.231663048,
		-0.0109971147, 0.231649041, -0.972737253
	)
	
	-- Точка возврата при отключении
	local RETURN_CFRAME = CFrame.new(
		10, 10.5880432, -9.99999905,
		-1, 0, 0,
		0, 1, 0,
		0, 0,-1
	)
	
	-- Переменные для управления полётом
	local bg, bv, flyConnection
	
	script.Parent.MouseButton1Click:Connect(function()
		local player   = game.Players.LocalPlayer
		local char     = player.Character or player.CharacterAdded:Wait()
		local root     = char:WaitForChild("HumanoidRootPart")
		local humanoid = char:FindFirstChildOfClass("Humanoid")
	
		if not status then
			-- Включаем UI-эффект
			Ts:Create(script.Parent.Parent.In, Info,   {BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			Ts:Create(script.Parent.Parent.In.Frame, XInfo, {Position = UDim2.new(0,24,0,0)}):Play()
			status = true
	
			-- Телепорт в стартовую точку
			root.CFrame = START_CFRAME
	
			-- Отключаем Humanoid, чтобы не мешал
			if humanoid then humanoid.PlatformStand = true end
	
			-- Создаём BodyGyro для фиксации ориентации
			bg = Instance.new("BodyGyro", root)
			bg.P         = 9e4
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
			bg.cframe    = root.CFrame
	
			-- Создаём BodyVelocity для движения
			bv = Instance.new("BodyVelocity", root)
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
	
			-- Запускаем Heartbeat-подключение для цикла вперёд/назад
			local elapsed = 0
			local direction = 1  -- 1 = вперёд, -1 = назад
			flyConnection = RunSvc.Heartbeat:Connect(function(dt)
				-- обновляем время
				elapsed = elapsed + dt
				if elapsed >= HALF_CYCLE then
					elapsed = elapsed - HALF_CYCLE
					direction = -direction
				end
				-- задаём скорость вдоль локального LookVector
				bv.velocity = root.CFrame.LookVector * MOVE_SPEED * direction
				-- удерживаем ориентацию
				bg.cframe = root.CFrame
			end)
	
		else
			-- Отключаем UI-эффект
			Ts:Create(script.Parent.Parent.In, Info,   {BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			Ts:Create(script.Parent.Parent.In.Frame, XInfo, {Position = UDim2.new(0,0,0,0)}):Play()
			status = false
	
			-- Прекращаем цикл полёта
			if flyConnection then
				flyConnection:Disconnect()
				flyConnection = nil
			end
	
			-- Восстанавливаем Humanoid
			if humanoid then humanoid.PlatformStand = false end
	
			-- Удаляем физические объекты
			if bg then bg:Destroy() end
			if bv then bv:Destroy() end
	
			-- Телепортируем обратно
			root.CFrame = RETURN_CFRAME
		end
	end)
	
end
coroutine.wrap(UHJJMK_fake_script)()
local function TPDJOQB_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.8}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err = pcall(function()
			local Players = game:GetService("Players")
			local player = Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local hrp = character:WaitForChild("HumanoidRootPart")
			local targetCFrame = CFrame.new(
				131.837906, 466.159576, -10.4474125, 0.646039188, 0, 0.763304353, 0, 1, 0, -0.763304353, 0, 0.646039188
			)
	
			-- Выполняем телепорт
			if hrp then
				hrp.CFrame = targetCFrame
			else
				warn("HumanoidRootPart не найден для телепортации.")
			end
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()
		end
	end)
	
end
