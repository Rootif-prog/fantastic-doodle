-- Scripts:

local function VZIKZS_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local MainEvent = ReplicatedStorage.Remotes.MainEvent -- RemoteEvent 
	
				MainEvent:FireServer(
					"Clicking",
					90000000000
				)
			end)
			wait()
		end
	end)
end
coroutine.wrap(VZIKZS_fake_script)()
local function YVQZ_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local MainEvent = ReplicatedStorage.Remotes.MainEvent -- RemoteEvent 
				MainEvent:FireServer(
					"OpenCase",
					"Free Case",
					1
				)
			end)
			wait()
		end
	end)
end
coroutine.wrap(YVQZ_fake_script)()
local function XTLBG_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local Players = game:GetService("Players")
	
				local MainEvent = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("MainEvent")
				local player = Players.LocalPlayer
				local inventory = player:WaitForChild("Inventory")
	
				-- Функция, которая продаёт все предметы
				local function sellAllItems()
					for _, itemValue in ipairs(inventory:GetChildren()) do
						-- Проверяем, что это именно NumberValue (имя — название оружия, value — цена)
						if itemValue:IsA("NumberValue") then
							local weaponName = itemValue.Name
							local price = itemValue.Value
							-- Вызываем серверный эвент продажи
							MainEvent:FireServer("SellItem", weaponName, price)
						end
					end
				end
	
				-- Запускаем функцию сразу (или привяжи к какой-то кнопке/событию)
				sellAllItems()
			end)
			wait()
		end
	end)
end
coroutine.wrap(XTLBG_fake_script)()
local function UAXTMA_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local MainEvent = ReplicatedStorage.Remotes.MainEvent -- RemoteEvent 
				MainEvent:FireServer(
					"OpenCase",
					tostring(_G.Stage),
					1
				)
			end)
			wait()
		end
	end)
end
coroutine.wrap(UAXTMA_fake_script)()
local function QFGTTEM_fake_script() -- TextBox.Controller 
	local script = Instance.new('LocalScript', TextBox)

	local TweenService = game:GetService("TweenService")
	local infoTween   = TweenInfo.new(1.3)
	local errorTween  = TweenInfo.new(0.7)
	
	local textBox = script.Parent
	
	-- Список кейсов: индекс → {name, price}
	local caseList = {
		[1]  = { name = "Free Case",               price = 0      },
		[2]  = { name = "Recoil Case",             price = 0.47   },
		[3]  = { name = "Fracture Case",           price = 0.62   },
		[4]  = { name = "Clutch Case",             price = 1.53   },
		[5]  = { name = "VIP Case",                price = 1.55   },
		[6]  = { name = "Prisma 2 Case",           price = 2.48   },
		[7]  = { name = "Revolver Case",           price = 5.41   },
		[8]  = { name = "Gamma 2 Case",            price = 6.95   },
		[9]  = { name = "Chroma Case",             price = 9.17   },
		[10] = { name = "Weapon Case 3",           price = 14.15  },
		[11] = { name = "Huntsman Weapon Case",    price = 17.87  },
		[12] = { name = "2013 Winter Case",        price = 19.63  },
		[13] = { name = "2014 Summer Case",        price = 20.53  },
		[14] = { name = "Operation Hydra Case",    price = 42.23  },
		[15] = { name = "Operation Bravo Case",    price = 81.28  },
		[16] = { name = "2013 Case",               price = 120.65 },
		[17] = { name = "Weapon Case",             price = 183.09 },
	}
	
	-- Цвета
	local defaultColor = Color3.new(0.239216, 0.239216, 0.239216)
	local focusColor   = Color3.new(0.439216, 0.439216, 0.439216)
	local errorColor   = Color3.new(0.698039, 0.0705882, 0.164706)
	
	-- При фокусе — подсветка
	textBox.Focused:Connect(function()
		TweenService:Create(textBox, infoTween, {BackgroundColor3 = focusColor}):Play()
	end)
	
	-- При потере фокуса — проверяем ввод
	textBox.FocusLost:Connect(function()
		-- вернём цвет к базовому
		TweenService:Create(textBox, infoTween, {BackgroundColor3 = defaultColor}):Play()
	
		local idx = tonumber(textBox.Text)
		local entry = caseList[idx]
	
		if idx and entry then
			-- Записываем выбранный кейс (имя) и цену, если надо
			_G.Stage      = entry.name
			_G.StagePrice = entry.price
			-- (тут можно добавить вызов MainEvent:FireServer, если хотите сразу продавать)
		else
			-- неверный ввод — моргаем красным
			TweenService:Create(textBox, errorTween, {BackgroundColor3 = errorColor}):Play()
			wait()
			TweenService:Create(textBox, errorTween, {BackgroundColor3 = defaultColor}):Play()
		end
	end)
	
end
coroutine.wrap(QFGTTEM_fake_script)()
local function PLLBQLC_fake_script() -- Trigger_5.Controller 
	local script = Instance.new('LocalScript', Trigger_5)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.8}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err = pcall(function()
			-- LocalScript в StarterPlayerScripts
	
			local Players     = game:GetService("Players")
			local localPlayer = Players.LocalPlayer
	
			-- Получаем HumanoidRootPart (ждём при необходимости)
			local function getRootPart()
				local char = localPlayer.Character or localPlayer.CharacterAdded:Wait()
				return char:WaitForChild("HumanoidRootPart")
			end
	
			-- Однократный «тап»
			local function touchPart(part, root)
				firetouchinterest(part, root, 0)
				firetouchinterest(part, root, 1)
			end
	
			-- Основная функция: по очереди от 1 до 9
			local function touchAllSequential()
				local root = getRootPart()
	
				for i = 1, 9 do
					-- ждем нужную часть
					local collectPart = workspace
						:WaitForChild("Obbies")
						:WaitForChild(tostring(i))
						:WaitForChild("Folder")
						:WaitForChild("Cash")
						:WaitForChild("Collect")
	
					-- делаем «тап»
					touchPart(collectPart, root)
	
					-- ждём, пока сервер обработает сбор этой части
					-- (предполагаем, что Collect удаляется или перемещается из иерархии)
					local timeout = 3
					local elapsed = 0
					while collectPart.Parent and elapsed < timeout do
						wait(0.1)
						elapsed = elapsed + 0.1
					end
	
					-- небольшая дополнительная пауза перед следующим
					wait(0.2)
				end
			end
	
			-- Запускаем при спавне
			local function onCharacter(char)
				-- даём мирку прогрузиться
				wait(0.5)
				touchAllSequential()
			end
	
			localCharacter = localPlayer.Character
			if localCharacter then
				onCharacter(localCharacter)
			end
			localPlayer.CharacterAdded:Connect(onCharacter)
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait()
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(1,1,1)}):Play()
		end
	end)
	
end
coroutine.wrap(PLLBQLC_fake_script)()
local function GTDNQL_fake_script() -- TextLabel_7.Controller 
	local script = Instance.new('LocalScript', TextLabel_7)

