local function EAHRF_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local player = game:GetService("Players").LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
				local rootPart = character:WaitForChild("HumanoidRootPart")
	
				-- Позиция броска (немного перед лицом)
				local throwFrom = rootPart.Position + Vector3.new(0, 1.5, 0) + (rootPart.CFrame.LookVector * 2)
	
				-- Направление броска (вперёд + немного вверх)
				local throwTo = throwFrom + (rootPart.CFrame.LookVector * 30) + Vector3.new(0, 0.5, 0)
	
				local Event = game.Players.LocalPlayer.Character.Grenade.Remote
				Event:FireServer(throwTo, throwFrom)
			end)
			wait()
		end
	end)
end
coroutine.wrap(EAHRF_fake_script)()
