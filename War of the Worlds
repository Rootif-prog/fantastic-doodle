-- Scripts:

local function PNJXSE_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	local Players = game.Players; local IYMouse = game.Players.LocalPlayer:GetMouse()
	FLYING = false
	QEfly = true
	iyflyspeed = 1
	vehicleflyspeed = 1
	local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
	local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
	
	local flyKeyDown = IYMouse.KeyDown:Connect(function(KEY)
		vfly = false
		if KEY:lower() == 'w' then
			CONTROL.F = (vfly and vehicleflyspeed or iyflyspeed)
		elseif KEY:lower() == 's' then
			CONTROL.B = - (vfly and vehicleflyspeed or iyflyspeed)
		elseif KEY:lower() == 'a' then
			CONTROL.L = - (vfly and vehicleflyspeed or iyflyspeed)
		elseif KEY:lower() == 'd' then 
			CONTROL.R = (vfly and vehicleflyspeed or iyflyspeed)
		elseif QEfly and KEY:lower() == 'e' then
			CONTROL.Q = (vfly and vehicleflyspeed or iyflyspeed)*2
		elseif QEfly and KEY:lower() == 'q' then
			CONTROL.E = -(vfly and vehicleflyspeed or iyflyspeed)*2
		end
		pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Track end)
	end)
	local flyKeyUp = IYMouse.KeyUp:Connect(function(KEY)
		if KEY:lower() == 'w' then
			CONTROL.F = 0
		elseif KEY:lower() == 's' then
			CONTROL.B = 0
		elseif KEY:lower() == 'a' then
			CONTROL.L = 0
		elseif KEY:lower() == 'd' then
			CONTROL.R = 0
		elseif KEY:lower() == 'e' then
			CONTROL.Q = 0
		elseif KEY:lower() == 'q' then
			CONTROL.E = 0
		end
	end)
	function sFLY(vfly)
		vfly = false
		repeat wait() until Players.LocalPlayer and Players.LocalPlayer.Character and Players.LocalPlayer.Character.HumanoidRootPart and Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
		repeat wait() until IYMouse
		if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
	
		local T = Players.LocalPlayer.Character.HumanoidRootPart
		local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
		local SPEED = 0
	
		local function FLY()
			FLYING = true
			local BG = Instance.new('BodyGyro')
			local BV = Instance.new('BodyVelocity')
			BG.P = 9e4
			BG.Parent = T
			BV.Parent = T
			BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
			BG.cframe = T.CFrame
			BV.velocity = Vector3.new(0, 0, 0)
			BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
			task.spawn(function()
				repeat wait()
					iyflyspeed = _G.Stage
					if not vfly and Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
						Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = true
					end
					if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
						SPEED = 50
					elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
						SPEED = 0
					end
					if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
						BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (CONTROL.F + CONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
						lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
					elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
						BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (lCONTROL.F + lCONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
					else
						BV.velocity = Vector3.new(0, 0, 0)
					end
					BG.cframe = workspace.CurrentCamera.CoordinateFrame
				until not FLYING
				CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
				SPEED = 0
				BG:Destroy()
				BV:Destroy()
				if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
					Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
				end
			end)
		end
		flyKeyDown = IYMouse.KeyDown:Connect(function(KEY)
			if KEY:lower() == 'w' then
				CONTROL.F = (vfly and vehicleflyspeed or iyflyspeed)
			elseif KEY:lower() == 's' then
				CONTROL.B = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif KEY:lower() == 'a' then
				CONTROL.L = - (vfly and vehicleflyspeed or iyflyspeed)
			elseif KEY:lower() == 'd' then 
				CONTROL.R = (vfly and vehicleflyspeed or iyflyspeed)
			elseif QEfly and KEY:lower() == 'e' then
				CONTROL.Q = (vfly and vehicleflyspeed or iyflyspeed)*2
			elseif QEfly and KEY:lower() == 'q' then
				CONTROL.E = -(vfly and vehicleflyspeed or iyflyspeed)*2
			end
			pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Track end)
		end)
		flyKeyUp = IYMouse.KeyUp:Connect(function(KEY)
			if KEY:lower() == 'w' then
				CONTROL.F = 0
			elseif KEY:lower() == 's' then
				CONTROL.B = 0
			elseif KEY:lower() == 'a' then
				CONTROL.L = 0
			elseif KEY:lower() == 'd' then
				CONTROL.R = 0
			elseif KEY:lower() == 'e' then
				CONTROL.Q = 0
			elseif KEY:lower() == 'q' then
				CONTROL.E = 0
			end
		end)
		FLY()
	end
	
	function NOFLY()
		FLYING = false
		if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
		if Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
			Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
		end
		pcall(function() workspace.CurrentCamera.CameraType = Enum.CameraType.Custom end)
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		if status then
			NOFLY()
			wait()
			sFLY()
		else
			NOFLY()
		end
	end)
end
coroutine.wrap(PNJXSE_fake_script)()
local function CXWCOTW_fake_script() -- TextBox.Controller 
	local script = Instance.new('LocalScript', TextBox)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.Focused:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.439216, 0.439216, 0.439216)}):Play()
		end)
	end)
	
	script.Parent.FocusLost:Connect(function()
		local sucess, err = pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			local textbox = script.Parent
			local text = textbox.Text
			
			local tonum = tonumber(text)
			
			if tonum ~= nil then
				_G.Stage = tonum
			else
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.698039, 0.0705882, 0.164706)}):Play()
				wait(1)
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			end
		end)
		
		if not sucess then print(err) end
	end)
end
coroutine.wrap(CXWCOTW_fake_script)()
local function EDJJQXZ_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x = TweenInfo.new(0.7)
	local status = false
	
	-- Настройки ESP
	_G.FriendColor = Color3.fromRGB(0, 0, 255)
	_G.EnemyColor = Color3.fromRGB(255, 0, 0)
	_G.UseTeamColor = true
	
	-- Инициализация ESP элементов
	local function CreateHolder()
		local holder = game.CoreGui:FindFirstChild("ESP_Holder")
		if not holder then
			holder = Instance.new("Folder")
			holder.Name = "ESP_Holder"
			holder.Parent = game.CoreGui
		end
		return holder
	end
	
	local Holder = CreateHolder()
	
	local function LoadCharacter(v)
		if not status then return end
		if v == game.Players.LocalPlayer then return end
	
		local function setup()
			if not v.Character then return end
	
			local vHolder = Holder:FindFirstChild(v.Name)
			if not vHolder then
				vHolder = Instance.new("Folder")
				vHolder.Name = v.Name
				vHolder.Parent = Holder
			end
	
			-- Box Handle
			local box = vHolder:FindFirstChild("Box") or Instance.new("BoxHandleAdornment")
			box.Name = "Box"
			box.Size = Vector3.new(1, 2, 1)
			box.Color3 = Color3.new(1, 1, 1)
			box.Transparency = 0.7
			box.ZIndex = 0
			box.AlwaysOnTop = false
			box.Visible = true
			box.Adornee = v.Character
			box.Parent = vHolder
	
			-- Name Tag
			local nameTag = vHolder:FindFirstChild("NameTag") or Instance.new("BillboardGui")
			nameTag.Name = "NameTag"
			nameTag.Size = UDim2.new(0, 200, 0, 50)
			nameTag.AlwaysOnTop = true
			nameTag.StudsOffset = Vector3.new(0, 1.8, 0)
			nameTag.Enabled = true
			nameTag.Adornee = v.Character:WaitForChild("Head")
			nameTag.Parent = vHolder
	
			local tag = nameTag:FindFirstChild("Tag") or Instance.new("TextLabel")
			tag.Name = "Tag"
			tag.Text = v.Name
			tag.TextSize = 15
			tag.TextStrokeTransparency = 0.4
			tag.Font = Enum.Font.SourceSansBold
			tag.BackgroundTransparency = 1
			tag.Size = UDim2.new(1, 0, 1, 0)
			tag.Parent = nameTag
	
			-- Highlight
			local highlight = vHolder:FindFirstChild("Highlight") or Instance.new("Highlight")
			highlight.Name = "Highlight"
			highlight.Adornee = v.Character
			highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
			highlight.Parent = vHolder
	
			-- Обновление цветов
			local function UpdateColors()
				if not status then return end
				local teamColor = _G.UseTeamColor and v.TeamColor.Color or
					(game.Players.LocalPlayer.Team == v.Team and _G.FriendColor or _G.EnemyColor)
	
				box.Color3 = teamColor
				tag.TextColor3 = teamColor
				highlight.FillColor = teamColor
			end
	
			UpdateColors()
			v:GetPropertyChangedSignal("TeamColor"):Connect(UpdateColors)
		end
	
		if v.Character then
			setup()
		end
		v.CharacterAdded:Connect(setup)
	end
	
	local function UnloadCharacter(v)
		local vHolder = Holder:FindFirstChild(v.Name)
		if vHolder then
			vHolder:Destroy()
		end
	end
	
	local function ToggleESP(enable)
		for _, v in ipairs(game.Players:GetPlayers()) do
			if enable then
				LoadCharacter(v)
			else
				UnloadCharacter(v)
			end
		end
	end
	
	-- Обработчик кнопки
	script.Parent.MouseButton1Click:Connect(function()
		status = not status
	
		-- Анимация кнопки
		if status then
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0,18,0,0)}):Play()
		else
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0,0,0,0)}):Play()
		end
	
		-- Управление ESP
		ToggleESP(status)
	
		-- Обработчики игроков
		if status then
			game.Players.PlayerAdded:Connect(function(v)
				LoadCharacter(v)
			end)
	
			game.Players.PlayerRemoving:Connect(function(v)
				UnloadCharacter(v)
			end)
		end
	end)
	
	-- Первоначальная загрузка
	ToggleESP(status)
end
coroutine.wrap(EDJJQXZ_fake_script)()
local function KXVLUA_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		if status then
			local plrlist = game.Players:GetPlayers()
			
			for i,v:Player in plrlist do
				if not v.Character:FindFirstChildOfClass("Highlight") and v.Character ~= game.Players.LocalPlayer.Character then
					local new = Instance.new("Highlight")
					new.FillTransparency = 0
					new.OutlineTransparency = 1
					new.Parent = v.Character
					new.Adornee = v.Character
					new.FillColor = Color3.new(0,0,1)
				end
			end
		else
			local plrlist = game.Players:GetPlayers()
	
			for i,v:Player in plrlist do
				if v.Character:FindFirstChildOfClass("Highlight") then
					v.Character:FindFirstChildOfClass("Highlight"):Destroy()
				end
			end
		end
	end)
end
