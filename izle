local Artifacts = Collect.Button({
	Text = "Take Artefacts",
	Callback = function()
		local success, err = pcall(function()
			local arts = {
				CFrame.new(-1350.38, -458.775238, -1580.62024, 1, 0, 0, 0, 1, 0, 0, 0, 1),
				CFrame.new(1405.88269, -250.722977, -1855.16284, 1, 0, 0, 0, 1, 0, 0, 0, 1),
				CFrame.new(-752.184326, 126.690788, -3171.6106, 1, 0, 0, 0, 1, 0, 0, 0, 1),
				CFrame.new(-1761.74756, -194.51709, -1306.67212, -0.885949731, 0.00651492085, 0.463735491, -0.00259297574, 0.99981612, -0.0189999882, -0.463773996, -0.0180354901, -0.885769963)
			}
			local arts_n = {"A","B","C","D"}
			local char : Character = game.Players.LocalPlayer.Character
			local root = char:FindFirstChild("HumanoidRootPart")
			local c = root.CFrame
			local revolver_cf = CFrame.new(472.481567, 164.817795, -1216.77148, -0.101358771, 2.70025069e-09, -0.99484992, 5.56263746e-09, 1, 2.14748819e-09, 0.99484992, -5.31632294e-09, -0.101358771)

			for position, artifact_c in pairs(arts) do
				local artifact_name = string.format("Artifact %s",arts_n[position])
				local tp_c = artifact_c
				if position ~= 4 then
					pcall(function()
						local art; for i,v in workspace.Map.Ignore.Tools:GetChildren() do if string.find(v.Name:lower(),artifact_name:lower()) then art = v break end end
						if art then
							tp_c = art.WorldPivot
						end
					end)
					for i = 1, 5 do root.CFrame = tp_c task.wait(0.01) end
					local is_f = false; local artifact = nil
					repeat wait() for i,v in workspace.Map.Ignore.Tools:GetChildren() do if string.find(v.Name:lower(),artifact_name:lower()) then is_f = true; artifact = v break end end until is_f 
					local args = {"#*@85799661851116949753", artifact.Name}
					for i = 1,2 do workspace:WaitForChild("Services"):WaitForChild("PickupTool"):InvokeServer(unpack(args)) wait(.5) end
					repeat wait() until char:FindFirstChild(artifact_name) or game.Players.LocalPlayer.Backpack:FindFirstChild(artifact_name)
					local art = nil
					if char:FindFirstChild(artifact_name) then
						art = char:FindFirstChild(artifact_name)
					else
						art = game.Players.LocalPlayer.Backpack:FindFirstChild(artifact_name)
						char.Humanoid:EquipTool(art)
						wait()
					end
					for i = 1, 5 do
						art:Activate() task.wait(.05)
					end
					wait()
				else 
					local artifact_name = string.format("Artifact %s",arts_n[position])
					for i =1, 5 do root.CFrame = revolver_cf task.wait(.03) end; local revo
					pcall(function() _G.SetN("Artifact D // SpineWare","Take the revolver, usually it spawns near you",7.5) end)
					repeat wait() for i,v in workspace.Map.Ignore.Tools:GetChildren() do if string.find(v.Name:lower(),"revolver") then pcall(function() if (v.Collision.Position - root.Position).Magnitude <= 12 then revo = v end  end) end end until revo
					local argssse = {"#*@85799661851116949753", revo.Name}
					for i = 1,2 do workspace:WaitForChild("Services"):WaitForChild("PickupTool"):InvokeServer(unpack(argssse)) wait(.5) end
					repeat wait() until char:FindFirstChild("Revolver") and char:FindFirstChild("Revolver"):FindFirstChildOfClass("RemoteEvent")
					local rev = char:FindFirstChild("Revolver")
					local kill_event = rev:FindFirstChild("@Kill")
					for i = 1, 5 do root.CFrame = artifact_c wait(.03) end
					repeat wait() until workspace.Threats:WaitForChild("Glass Container") and char:FindFirstChild("Revolver"):FindFirstChildOfClass("RemoteEvent")
					local glass = workspace.Threats:WaitForChild("Glass Container"); local skip = false
					if glass.Humanoid.Health >= 0 then skip = true end
					if not skip then task.wait(.25)
						pcall(function() _G.SetN("Artifact D // SpineWare","Take the revolver, usually it spawns near you",7.5) end)
						task.wait(1.25)
						kill_event:FireServer(glass) end
					local is_f = false; local artifact = nil;
					repeat task.wait() for i,v in workspace.Map.Ignore.Tools:GetChildren() do if string.find(v.Name:lower(),artifact_name:lower()) then is_f = true; artifact = v break end end until is_f 
					for i = 1, 5 do root.CFrame = artifact.WorldPivot end
					local argsss = {"#*@85799661851116949753", artifact.Name}
					for i = 1,2 do workspace:WaitForChild("Services"):WaitForChild("PickupTool"):InvokeServer(unpack(argsss)) wait(.5) end
					repeat wait() until char:FindFirstChild(artifact_name) or game.Players.LocalPlayer.Backpack:FindFirstChild(artifact_name)
					local art = nil
					if char:FindFirstChild(artifact_name) then
						art = char:FindFirstChild(artifact_name)
					else
						art = game.Players.LocalPlayer.Backpack:FindFirstChild(artifact_name)
						char.Humanoid:EquipTool(art)
						task.wait()
					end
					for i = 1, 5 do
						art:Activate() task.wait(.05)
					end
					task.wait()
				end
			end
		end)
	end
})
local Collectibles = Collect.Button({
	Text = "Collect Collectibles",
	Callback = function()
		lastc = game['Players']['LocalPlayer']['Character']['HumanoidRootPart']['CFrame']
		for _, v : Model | UnionOperation in workspace.Map.Ignore.Collectibles:GetChildren() do
			a,b = pcall(function()
				local click;
				for i,x in v:GetDescendants() do
					if x:IsA("ClickDetector") then
						click = x;
					end
				end
				for i = 1,  5 do
					fireclickdetector(click)
					task.spawn(function()
						parent = click['Parent']:IsA("Model") and click['Parent']['PrimaryPart'] or click['Parent']
						firetouchinterest(game['Players']['LocalPlayer']['Character']['HumanoidRootPart'], parent, 0)
						task.wait()
						firetouchinterest(game['Players']['LocalPlayer']['Character']['HumanoidRootPart'], parent, 1)
					end)
					xpcall(function()
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.WorldPivot
					end,function()
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
					end)
					task.wait(.025)
				end
				task.wait(0.05)
			end)
		end
		game['Players']['LocalPlayer']['Character']['HumanoidRootPart']['CFrame'] = lastc
	end,
	Menu = { 
		Tip = function()
			X.Banner({Text = "Use only with diving pack!!!"})
		end
	}
})
local Files = Collect.Button({
	Text = "Collect Files",
	Callback = function()
		local success, err = pcall(function()
			local plr : Instance = game.Players.LocalPlayer.Character
			local last = plr.HumanoidRootPart.CFrame
			for _, v in workspace.Map.Ignore.Files:GetChildren() do
				local clicker;
				for __, click in v:GetDescendants() do
					if click:IsA("ClickDetector") then
						clicker = click
					end
				end
				for i = 1, 10 do
					task.spawn(function()
						parent = clicker['Parent']:IsA("Model") and clicker['Parent']['PrimaryPart'] or clicker['Parent']
						game['Players']['LocalPlayer']['Character']['HumanoidRootPart']['CFrame'] = parent['CFrame']
					end)
					fireclickdetector(clicker)
					task.wait(.05)
				end
			end
			plr.HumanoidRootPart.CFrame = last
		end)
	end
})
local Marble = Collect.Button({
	Text = "Collect Marble Pieces",
	Callback = function()
		local success, err = pcall(function()
			for _,v in workspace.Map.Main:GetChildren() do
				if string.find(v.Name,"Marble Piece") then
					local breakf = false					
					pcall(function()
						local StarterGui = game:GetService("StarterGui") 
						local bindable = Instance.new("BindableFunction")	
						function bindable.OnInvoke(response)
							if response == "Break" then
								breakf = true
							else								
							end
						end
						StarterGui:SetCore("SendNotification", {
							Title = "Break function // SpineWare",
							Text = "Attention! If ur inventory is full, then turn on 'Inf Storage' or break function",
							Duration = 9e9,
							Callback = bindable,
							Button1 = "Break",
							Button2 = "Ignore"
						})
						repeat game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.WorldPivot  task.wait() until breakf or game.Players.LocalPlayer.Backpack:FindFirstChild(v.Name) or game.Players.LocalPlayer.Character:FindFirstChild(v.Name)
					end)
				end
			end
		end)
	end
})
