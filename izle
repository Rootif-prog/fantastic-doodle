local Reward = Giver.Dropdown({
	Text = "Reward",
	Options = {
		"Note",
		"Be stronger",
		"BCorp shit"
	},
	Callback = function(reward)
		if reward == "Note" then
			toggles_list['ChoosedReward'] = "for access"
		elseif reward == "BCorp shit" then
			toggles_list['ChoosedReward'] = "to wipe out my"
		elseif reward == "Be stronger" then
			toggles_list['ChoosedReward'] = "to become stronger"
		end
	end
})
local RewardCount;
RewardCount = Giver.TextField({
	Text = "Rewards count: 1",
	Callback = function(count)
		if tonumber(count) ~= nil and tonumber(count) ~= 0 then
			toggles_list['RewardCount'] = tonumber(count) 
			RewardCount:SetText(string.format("Rewards count: %d", toggles_list['RewardCount']))
		end
	end,
})
local TalkToDragon = Giver.Button({
	Text = "Start dialogue with dragon",
	Callback = function()
		local touching = workspace.Map.Ignore.SpecialRegions.DragonDialogue
		local hrp = game['Players']['LocalPlayer']['Character']['HumanoidRootPart']

		task.spawn(function()
			firetouchinterest(hrp, touching, 0)
			task.wait()
			firetouchinterest(hrp, touching, 1)
		end)
	end
})
