local EnterTeleport = Collect.Button({
	Text = "Take Teleporter Enterance", 
	Callback = function()
		local success, err = pcall(function()
			local teleport = workspace.Map.Ignore.Debris["Teleport Entrance"]
			local lastpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			repeat
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = teleport.WorldPivot
				for i = 1, 10 do
					teleport:FindFirstChildOfClass("RemoteEvent"):FireServer()
					task.wait(.05)
				end
				task.wait()
			until game.Players.LocalPlayer.Backpack:FindFirstChild("Teleporter Entrance")
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastpos
		end)
		if not success then X.Banner({Text = "Not Success :("}) end
	end
})
