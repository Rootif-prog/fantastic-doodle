				local function bypass_tp(cframe: CFrame)
					local char: Model = game['Players']['LocalPlayer']['Character']
					local root: BasePart = char['HumanoidRootPart'] or char:WaitForChild("HumanoidRootPart", 9e9)
					local anomaly: BasePart = workspace['Map']['Ignore']['SpecialRegions']['Anomaly3']
					repeat task.wait() until anomaly['PointLight']['Enabled']
					repeat root['CFrame'] = anomaly['CFrame']; firetouchinterest(anomaly, root, 0); task.wait(.01); firetouchinterest(anomaly, root, 1) until not anomaly:FindFirstChildOfClass("PointLight")['Enabled']
					root['CFrame'] = cframe
				end
