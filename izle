local ChoosedPlayerDrop = Troll.Dropdown({
	Text = "Player",
	Options = {},
	Callback = function(choosed_player)
		if game['Players'][choosed_player] then
			toggles_list['TrolledPlayer']['Value'] = game['Players'][choosed_player]
		end
	end
})
local Leviaphan = Troll.Button({
	Text = "Die by leviathan near choosen player",
	Menu = {
		READ_IT = function()
			X.Banner({Text = "This function will kill you! The leviathan will jump and eat you near that player! If script cant find locations, fly around the water where leviathan usually jumps (hangar recommended)"})
		end
	},
	Callback = function()
		local spawns = {{CFrame = CFrame.new(-3152.80005, -12.0000038, -2915, 1, 0, 0, 0, 1, 0, 0, 0, 1)}, {CFrame = CFrame.new(-3226.01953, -36.3047562, -2918.7959, -0.0586063899, -0.147178605, -0.98737216, 0, 0.989072204, -0.147432014, 0.998281181, -0.00864045788, -0.0579659529)}}
		local hrp = game['Players']['LocalPlayer']['Character']['HumanoidRootPart']

		local player = toggles_list['TrolledPlayer']['Value']
		if player ~= nil then
			local jump_position;
			if player == game['Players']['LocalPlayer'] then
				jump_position = hrp['CFrame']
			else
				jump_position = "nonself"
			end

			for _, jump in spawns do
				for attempt = 1, 2 do
					game:GetService("TweenService"):Create(hrp, TweenInfo.new(0.05), {CFrame = jump['CFrame'] - Vector3.new(0, 2, 0)}):Play()
					task.wait(0.4)
					hrp['CFrame'] = jump_position == "nonself" and player['Character']['HumanoidRootPart']['CFrame'] or jump_position
					task.wait(.1)
					game:GetService("TweenService"):Create(hrp, TweenInfo.new(0.05), {CFrame = jump['CFrame'] - Vector3.new(15, 12, 15)}):Play()
					task.wait(0.5)
					hrp['CFrame'] = jump_position == "nonself" and player['Character']['HumanoidRootPart']['CFrame'] or jump_position
				end
				break
			end
		else
			X.Banner({Text = "You havent choosed a player!"})
		end
	end
})
local Acid = Troll.Button({
	Text = "Touch Acid",
	Menu = {
		Tip = function()
			X.Banner({Text = "Use only with a shield pack or it`ll kill you"})
		end
	},
	Callback = function()
