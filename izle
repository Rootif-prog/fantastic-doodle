gethui = function()
	return game['CoreGui']
end

local toggles_list = {
	ESP = Instance.new("BoolValue"),
	ESPEnabled = {
		Players = Instance.new("BoolValue"),
		Mercenaries = Instance.new("BoolValue"),
		Parasites = Instance.new("BoolValue")
	},
	WalkSpeed = Instance.new("IntValue"),
	JumpPower = Instance.new("IntValue"),
	SmartStorage = Instance.new("BoolValue"),
	ItemSeen = Instance.new("BoolValue"),
	InfStorage = Instance.new("BoolValue"),
	Noclip = Instance.new("BoolValue"),
	AntiSpotted = Instance.new("BoolValue"),
	TrollPlayer = Instance.new("BoolValue"),
	TrolledPlayer = Instance.new("ObjectValue"),
	BuddiesTroll = false,
	AutoAnswer = false,
	AutoTPGiver = false,
	ChoosedReward = "",
	RewardCount = 1,
	HighlightsEnabled = false,
	HighPlayer = Instance.new("Color3Value"),
	HighMerc = Instance.new("Color3Value"),
	HighParasite = Instance.new("Color3Value")
}

local Functions = {
	Mineshaft = function()
		local success, err = pcall(function()
			pcall(function ()
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Map.Main.PylonScreenEarth.Button.WorldPivot
				repeat task.wait() until workspace.Map.Main.PylonScreenEarth.Button:FindFirstChild("Main")
			end)

			local click = workspace.Map.Main.PylonScreenEarth.Button.Main.ClickDetector

			repeat
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = click.Parent.CFrame + Vector3.new(0,2,0)
				fireclickdetector(click)
				task.wait(.03)
			until click.Parent.Material ~= Enum.Material.Neon
		end)
	end,
	Water = function()
		local click = workspace.Map.Main.LeviathanFragments.State.Solved

		for i = 1, 15 do
			pcall(function()  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=workspace.Map.Main.PerspectiveTriggerModel.WorldPivot   end)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Map.Main.PerspectiveTriggerModel.PerspectiveTrigger.CFrame + Vector3.new(0,5,0)
			click:FireServer()
			task.wait(.01)
		end
	end,
	ApeCity = function()
		local lastpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

		for _,v in workspace.Map.Main.TheGlassDoorPuzzle:GetChildren() do
			if v:IsA("Model") and string.find(v.Name,"Plate") then
				a,b = pcall(function()
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.WorldPivot +Vector3.new(0,1,0)
					repeat task.wait() until v:FindFirstChild("Base") and v:FindFirstChild("Gem")
					repeat
						firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Base,0)
						task.wait()
						firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Base,1)
						task.wait()
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.WorldPivot +Vector3.new(0,0,0)
					until v:FindFirstChild("Gem").Material == Enum.Material.Neon
				end)
			end
		end

		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastpos
	end,
	Shields = function()
		local success, err = pcall(function()
			local lastpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			local list = {
				{workspace.Map.Main.LeechContainer},
				{workspace.Map.Main.HornetContainer},
				{workspace.Map.Main.FrogContainer}
			}
			for _, container in list do
				local plr = game.Players.LocalPlayer.Character
				local breakf = false
				local StarterGui = game:GetService("StarterGui") 
				local bindable = Instance.new("BindableFunction")
				function bindable.OnInvoke(response)
					if response == "Break" then
						breakf = true
					end
				end
				StarterGui:SetCore("SendNotification", {
					Title = "Break function // SpineWare",
					Text = "Attention! Maybe this alien already not in container",
					Duration = 20,
					Callback = bindable,
					Button1 = "Break",
					Button2 = "Ignore"
				})

				repeat plr.HumanoidRootPart.CFrame = container[1].WorldPivot task.wait() until not game.Players.LocalPlayer.GameplayPaused and container[1]:FindFirstChild("Handle") or breakf or task.wait(10)
				repeat
					plr.HumanoidRootPart.CFrame = container[1]:FindFirstChild("Handle").CFrame
					fireclickdetector(container[1]:FindFirstChild("Handle"):FindFirstChild("ClickDetector"))
					task.wait() 
				until container[1]:FindFirstChild("Handle"):FindFirstChild("Sabotaged").Value == true or breakf
			end
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastpos
		end)
	end,
	Levers = function()
		local success, err = pcall(function()
			local plr: Instance = game.Players.LocalPlayer.Character
			for _, v in workspace.Map.Main:GetChildren() do
				if v.Name == "ChainGate" then
					pcall(function ()
						plr.HumanoidRootPart.CFrame = v.WorldPivot
						plr.HumanoidRootPart.CFrame = v.Lever.Handle.LeverMain.CFrame
					end)
					repeat
						task.wait()
					until v:FindFirstChild("Lever")
					for i= 1, 10 do
						local click = v.Lever.Handle:FindFirstChild("LeverMain"):FindFirstChildOfClass("ClickDetector")
						fireclickdetector(click)
						task.wait(.05)
					end
					task.wait(.1)
				end
			end
		end)
	end,
}
task.spawn(function()
    pcall(function()
		local conn; conn = game['Players']['LocalPlayer']['Character']['@Alerts']['ChildAdded']:Connect(function()
			conn:Disconnect()
	        local value = Instance.new("Color3Value")
	        value['Value'] = Color3.fromRGB(0, 255, 0)
	        value['Name'] = "https://discord.gg/Pk7VQqZvZr"
	        value['Parent'] = game['Players']['LocalPlayer']['Character']['@Alerts']   
	        task.delay(2.5, function()
	            value:Destroy()
	            local value = Instance.new("Color3Value")
	            value['Value'] = Color3.fromRGB(0, 255, 0)
	            value['Name'] = "ORWALKEREDMZ COOKED"
	            value['Parent'] = game['Players']['LocalPlayer']['Character']['@Alerts']  
	            task.wait(2.5)
	            value:Destroy()
	        end)
		end)
    end)
end)
