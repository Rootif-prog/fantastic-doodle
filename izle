local jsons = game:GetService("HttpService")
local function refresh_hitbox_saving_file()
	local success, error = pcall(function()
		writefile('SpineWare/Isle/settings.json', jsons:JSONEncode({
			PlayerHighlightColor = {toggles_list['HighPlayer']['Value']['R'], toggles_list['HighPlayer']['Value']['G'], toggles_list['HighPlayer']['Value']['B']},
			MercenariesHiglightColor = {toggles_list['HighMerc']['Value']['R'], toggles_list['HighMerc']['Value']['G'], toggles_list['HighMerc']['Value']['B']},
			ParasitesHiglightColor = {toggles_list['HighParasite']['Value']['R'], toggles_list['HighParasite']['Value']['G'], toggles_list['HighParasite']['Value']['B']},
			ReadIt = "HI GUYS GUYZZ KSTOPICSTARTER COOKING TS SHIT"
		}))
	end)
	if not success then print(error) end
end

local dropdown_esp = ESP.ChipSet({
	Text = "Entities",
	Options = {
		Players = false,
		Parasites = false,
		Mercenaries = false
	},
	Callback = function(value)
		toggles_list['ESPEnabled']['Players']['Value'] = value['Players']
		toggles_list['ESPEnabled']['Parasites']['Value'] = value['Parasites']
		toggles_list['ESPEnabled']['Mercenaries']['Value'] = value['Mercenaries']
	end,
})
local toggle_higlight = ESP.Toggle({
	Text = "Highlights Enabled",
	Callback = function(boolean)
		toggles_list['HighlightsEnabled'] = boolean
	end,
	Enabled = false
})
toggles_list['HighPlayer']['Value'] = Color3.fromRGB(49, 204, 80)
toggles_list['HighMerc']['Value'] = Color3.fromRGB(204, 104, 38)
toggles_list['HighParasite']['Value'] = Color3.fromRGB(204, 11, 53)

-- file system (like saving the colors of highlights rn)
local main_folder = isfolder('SpineWare')
local d_folder = isfolder('SpineWare/Isle')
local p_f = isfile('SpineWare/Isle/settings.json')
if not main_folder then
	makefolder('SpineWare')
end
if not d_folder then
	makefolder('SpineWare/Isle')
end
if not p_f then
	writefile('SpineWare/Isle/settings.json', jsons:JSONEncode({
		PlayerHighlightColor = {49, 204, 80},
		MercenariesHiglightColor = {204, 104, 38},
		ParasitesHiglightColor = {204, 11, 53},
		ReadIt = "HI GUYS GUYZZ KSTOPICSTARTER COOKING TS SHIT"
	}))
else
	local s = jsons:JSONDecode(readfile('SpineWare/Isle/settings.json'))
	local players_color = Color3.new(s['PlayerHighlightColor'][1], s['PlayerHighlightColor'][2], s['PlayerHighlightColor'][3])
	local Mercenaries_color = Color3.new(s['MercenariesHiglightColor'][1], s['MercenariesHiglightColor'][2], s['MercenariesHiglightColor'][3])
	local Parasites_color = Color3.new(s['ParasitesHiglightColor'][1], s['ParasitesHiglightColor'][2], s['ParasitesHiglightColor'][3])
	toggles_list['HighPlayer']['Value'] = players_color
	toggles_list['HighMerc']['Value'] = Mercenaries_color
	toggles_list['HighParasite']['Value'] = Parasites_color
end


local color_picker_players = ESP.ColorPicker({
	-- hi genius, if u are one of isle devs - u arent genius... u are dumbass (like a blioooz). btw urwalkerdmz - my username while i was writin that script 
	Text = "Player Highlight Color",
	Default = toggles_list['HighPlayer']['Value'],
	Callback = function(new_color: Color3)
		toggles_list['HighPlayer']['Value'] = new_color
		refresh_hitbox_saving_file()
	end,
})
local color_picker_merc = ESP.ColorPicker({
	Text = "Mercenaries Highlight Color",
	Default = toggles_list['HighMerc']['Value'],
	Callback = function(new_color: Color3)
		toggles_list['HighMerc']['Value'] = new_color
		refresh_hitbox_saving_file()
	end,
})
local color_picker_paras = ESP.ColorPicker({
	Text = "Parasites Highlight Color",
	Default = toggles_list['HighParasite']['Value'],
	Callback = function(new_color: Color3)
		toggles_list['HighParasite']['Value'] = new_color
		refresh_hitbox_saving_file()
	end,
})
local MapMethod = 1
local SmartLoading = false

local Map = X.New({
	Title = "Map"
})
local MapLoad = Map.Button({
	Text = "Load Map by choosen method",
	Callback = function()
		if MapMethod == 1 then
			if SmartLoading then
				for i,v in game:GetService("ReplicatedStorage").Markers:GetChildren() do
					if v:FindFirstChild('TextLabel', true).Text ~= "Mirror World" then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
						task.wait(0.5)
						repeat task.wait() until game.Players.LocalPlayer.GameplayPaused == false
					end
				end
			else
				for i,v in game:GetService("ReplicatedStorage").Markers:GetChildren() do
					if v:FindFirstChild('TextLabel', true).Text ~= "Mirror World" then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
						task.wait(0.5)
					end
				end
			end
		else
			if SmartLoading then
				for i,v in game:GetService("ReplicatedStorage").Markers:GetChildren() do
					if v:FindFirstChild('TextLabel', true).Text ~= "Mirror World" then
						game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(0.5),{CFrame = v.CFrame}):Play()
						task.wait(0.6)
						repeat task.wait() until game.Players.LocalPlayer.GameplayPaused == false
					end
				end
			else
				for i,v in game:GetService("ReplicatedStorage").Markers:GetChildren() do
					if v:FindFirstChild('TextLabel', true).Text ~= "Mirror World" then
						game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(0.5),{CFrame = v.CFrame}):Play()
						task.wait(0.6)  
					end
				end             
			end
		end
	end
})
local SmartMethod = Map.Toggle({
	Text = "Wait till location was loaded?",
	Callback = function(status)
		SmartLoading = status
	end,
	Enabled = false,
	Menu = {
		Why = function(self)
			X.Banner({
				Text = "Items may be not loaded if location wasnt loaded. So usually many items wont be loaded. But this method takes 2x time than basic method"
			})
		end
	}
})
local Drop_Map = Map.Dropdown({
	Text = "Loading method",
	Options = {
		"Tween [Greater]",
		"Teleport"
	},
	Callback = function(option)
		if option == "Teleport" then
			MapMethod = 1
		else
			MapMethod = 2
		end
	end
})
local RemoveTrees = Map.Button({
	Text = "Remove all Trees",
	Callback = function()
		local success, err = pcall(function()
			for _, v in workspace.Map.Ignore:GetChildren() do
				if string.find(v.Name,"Trees") then
					v:Destroy()
				end
			end
		end)
	end
})
local RemoveGlass = Map.Button({
	Text = "Remove Glass Containers",
	Callback = function()
		local success, err = pcall(function()
			for _, v in workspace.Threats:GetChildren() do
				if string.find(v.Name:lower(),"glass") then
					v:Destroy()
				end
			end
		end)
	end
})
local RandomTP = Map.Button({
	Text = "Random TP [Portals]",
	Callback = function()
		local success, err = pcall(function()
			v = workspace.Map.Ignore.SpecialRegions:FindFirstChild("Anomaly")
			if v:FindFirstChildOfClass("PointLight").Enabled == true then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
			else
				for _, x in workspace.Map.Ignore.SpecialRegions:GetChildren() do
					if x.Name == "Anomaly" and x:FindFirstChildOfClass("PointLight").Enabled == true then
						v = x 
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
						return
					end
				end
			end
		end)
	end
})
