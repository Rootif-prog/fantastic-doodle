	local dialogues = plrgui['MainGui']['Dialogue']
	local shablon = {
		{"one of them bears hypnotising eyes", "gem"},
		{"one of them grants an escape from this place", "feathered"},
		{"one of them strikes from the hidden depths of the ocean", "scaled"},
		{"you right now", "forest"},
		{"time did my house appear here", "8:00"},
		{"guaranteed to spawn in facility", "glowstick"},
		{"Calamity, comity or curiosity?", "curiosity"},
		{"the natural human", "who are you"},
		{"i`m bored", "CLICKING"},
		{"time you spend", "CLICKING"},
		{"hearing more", "Yes"},
		{"Tick tock", "CLICKING"},
		{"something for you", "CLICKING"},
		{"something I'd like to", "CLICKING"},
		{"My move", "CLICKING"},
		{"My turn", "CLICKING"},
		{"very well", "CLICKING"},
		{"we will", "CLICKING"},
		{"you may", "CLICKING"},
		{"consider", "CLICKING"},
	}
	while 1 do
		if toggles_list['AutoAnswer'] then
			local question = ""
			local answers = {}
			if dialogues and dialogues:FindFirstChild('DialogueFrame') then
				local success, error = pcall(function()
					question = dialogues['DialogueFrame']:FindFirstChildOfClass("TextLabel")['Text']
					for _, option in dialogues['OptionFrame']:GetChildren() do
						if option:IsA("TextButton") then
							table.insert(answers, {option, option['Text']:lower()})
						end
					end  
					if question:lower():match("powered") or question:lower():match("been used") then 
						for _, value_power in workspace['Power']:GetChildren() do
							if question:lower():match(value_power['Name']:lower()) then
								for index, answer_var in answers do
									if answer_var[2]:lower() == (value_power['Value'] and "Yes" or "No"):lower() then
										answer_var[1]['TextColor3'] = Color3.new(0, 1, 0)
										firesignal(answer_var[1]['MouseButton1Up'])
										break
									end
								end
							end
						end
					elseif question:lower():match("active") then
						for _, value_power in workspace['Pylons']:GetChildren() do
							if question:lower():match("mineshaft") then
								question = "earth"
							elseif question:lower():match("radio tower") then
								question = "water"
							else
								question = "air"
							end
							if question:lower():match(value_power['Name']:lower()) then
								for index, answer_var in answers do
									if answer_var[2]:lower() == (value_power['Value'] and "Yes" or "No"):lower() then
										answer_var[1]['TextColor3'] = Color3.new(0, 1, 0)
										firesignal(answer_var[1]['MouseButton1Up'])
										break
									end
								end
							end
						end
					elseif question:lower():match("day") then
						local day = workspace['Weather']['Day']['Value']
						for index, answer_var in answers do
							if answer_var[2]:match(string.format('day %d', day)) then
								answer_var[1]['TextColor3'] = Color3.new(0, 1, 0)
								firesignal(answer_var[1]['MouseButton1Up'])
								break
							end
						end
					elseif question:lower():match('how many merc') then
						count = -1
						for __, object in worspace['AIHunter']:GetChildren() do
							if object:IsA("Model") then
								count += 1
							end
						end
						if not MercenariesCountStatused then
							X.Banner({Text = string.format("BIG DADDY`S COUNT ON ISLAND: %d", count)})
							MercenariesCountStatused = true
							task.delay(1.5, function()
								MercenariesCountStatused = false
							end)
						end
					end
					for i, shablon_variant in shablon do
						if (question:lower()):match(shablon_variant[1]:lower()) then
							for __, answer_variant in answers do
								if answer_variant[2] == "clicking" then
									firesignal(dialogues['DialogueFrame']['TextButton']['MouseButton1Up'])
									task.wait()
									break
								end
								if answer_variant[2]:lower():match(shablon_variant[2]:lower()) then
									answer_variant[1]['TextColor3'] = Color3.new(0, 1, 0)
									firesignal(answer_variant[1]['MouseButton1Up'])
									task.wait()
									break
								end
							end
						end
					end
				end)
			end
			if not success then print(error) end
		end
		if toggles_list['AutoTPGiver'] then
			if game:GetService("Lighting")['ClockTime'] >= 20 then
				if not teleportedToGiverXuesos then 
					teleportedToGiverXuesos = true
					for i = 1, 3 do 
						game['Players']['LocalPlayer']['Character']['HumanoidRootPart']['CFrame'] = CFrame.new(-770.85437, 4.60989952, 413.090179, -0.649261534, 6.0111347e-08, 0.760565221, -2.61333e-09, 1, -8.12659877e-08, -0.760565221, -5.47504868e-08, -0.649261534) + Vector3.new(0, 1.5, 0)
						task.wait(.1)
					end
				end
			else
				teleportedToGiverXuesos = false
			end
		end
		task.wait(.1)
	end
end)

--highlight system
local main_size = Vector3.new(3, 6, 3) -- we will be economing this shit!!!!!!!!!!!!!!
local exception = {}
local higlights = {} -- nneded for storing highlight parents and deleting them
local tables = {}

tables['_find'] = function(table_, searching)
	for _, v in table_ do
		if v == searching then
			return true
		end
	end	
	return false
end
local remove_higlight = function(character: Model)
	for _, desc: Part in higlights do
		if desc == character then
			pcall(function()
				desc:Destroy()
			end)
			pcall(function()
				table.remove(exception, character)
			end)
		end
	end
end
local return_type = function(character: Model)
	if game['Players']:GetPlayerFromCharacter(character) ~= nil then
		return "HighPlayer"
	elseif character:FindFirstAncestor("AIHunter") then
		return "HighMerc"
	end
	return "HighParasite"
end
local add_highlight = function(character: Model)
	local success, error = pcall(function()
		local highlight_new: Highlight = Instance.new("Highlight", character)
		highlight_new['FillTransparency'] = 0.7
		highlight_new['OutlineTransparency'] = 1
		highlight_new['FillColor'] = toggles_list[return_type(character)]['Value']
		toggles_list[return_type(character)]['Changed']:Connect(function(new_color)
			highlight_new['FillColor'] = toggles_list[return_type(character)]['Value']
		end)
		table.insert(higlights, highlight_new)
		table.insert(exception, character)
	end)
	if not success then print(error) end
end
task.spawn(function()
	while true do
		if toggles_list['HighlightsEnabled'] and toggles_list['ESP']['Value'] then
			for _, player: Player in game['Players']:GetPlayers() do
				if player['Character'] and player ~= game['Players']['LocalPlayer'] then
					if player['Character']:FindFirstChildOfClass("Humanoid")['Health'] <= 0 then 
						remove_higlight(player['Character'])
					end
					if not tables['_find'](exception, player['Character']) then
						add_highlight(player['Character'])
					end
				end
			end
			game['Players']['PlayerAdded']:Connect(function(player)
				if player['Character'] and player ~= game['Players']['LocalPlayer'] then
					if player['Character']:FindFirstChildOfClass("Humanoid")['Health'] <= 0 then 
						remove_higlight(player['Character'])
					end
					if not tables['_find'](exception, player['Character']) then
						add_highlight(player['Character'])
					end
				end
			end)
			for _, player in game['Workspace']['AIHunter']:GetChildren() do
				if player ~= nil and player:FindFirstChild("HumanoidRootPart") then
					if player:FindFirstChildOfClass("Humanoid") and player:FindFirstChildOfClass("Humanoid")['Health'] <= 0 then 
						remove_higlight(player)
					end
					if not tables['_find'](exception, player) then
						add_highlight(player)
					end
				end
			end
			game['Workspace']['AIHunter']['ChildAdded']:Connect(function(player)
				if player ~= nil and player:FindFirstChild("HumanoidRootPart") then
					if player:FindFirstChildOfClass("Humanoid") and player:FindFirstChildOfClass("Humanoid")['Health'] <= 0 then 
						remove_higlight(player)
					end
					if not tables['_find'](exception, player) then
						add_highlight(player)
					end
				end
			end)
			for _, player in game['Workspace']['Threats']:GetChildren() do
				if player ~= nil and player:FindFirstChild("HumanoidRootPart") then
					if player:FindFirstChildOfClass("Humanoid") and player:FindFirstChildOfClass("Humanoid")['Health'] <= 0 then 
						remove_higlight(player)
					end
					if not tables['_find'](exception, player) then 
						add_highlight(player)
					end
				end
			end
			game['Workspace']['Threats']['ChildAdded']:Connect(function(player)
				if player ~= nil and player:FindFirstChild("HumanoidRootPart") then
					if player:FindFirstChildOfClass("Humanoid") and player:FindFirstChildOfClass("Humanoid")['Health'] <= 0 then 
						remove_higlight(player)
					end
					if not tables['_find'](exception, player) then 
						add_highlight(player)
					end
				end
			end)
		else
			if #higlights > 0 then
				for _, v in higlights do
					pcall(function()
						v:Destroy()
					end)
					pcall(function()
						table.remove(exception, v['Parent'])
					end)
				end
			end
		end
		-- all of variables named player, cuz i dont want to waste my time on it...
		task.wait(.15)
	end
end)
