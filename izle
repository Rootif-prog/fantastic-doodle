local Noclip = Player_Tab.Toggle({
	Text = "Noclip",
	Callback = function(choice)
		toggles_list['Noclip']['Value'] = choice
		Noclip_ever_activated = true
	end,
	Enabled = false
})
local AntiFlag = Player_Tab.Button({
	Text = "Anti Fly Flag",
	Callback = function()
		X.Banner({Text = "Fly must be off"})
		local char = game['Players']['LocalPlayer']['Character']
		local hrp = char['HumanoidRootPart']
		if hrp then
			local ray = Ray.new(hrp.Position, Vector3.new(0, -1000, 0))
			local hit, pos = workspace:FindPartOnRay(ray, char)
			local targetY = hit and pos.Y or false
			local finishpos = Vector3.new(hrp.Position.X, targetY, hrp.Position.Z)
			for i = 1, 5 do
				game:GetService("TweenService"):Create(hrp, TweenInfo.new(.2), {CFrame = CFrame.new(finishpos)}):Play()
				task.wait(.4)
			end
		end
	end
})
