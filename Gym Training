local function ENLOZXY_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local DamageIncreaseOnClickEvent = ReplicatedStorage.Events.DamageIncreaseOnClickEvent
				DamageIncreaseOnClickEvent:FireServer(
					{
						tableNumber = 13,
						stageNumber = _G.Stage1
					}
				)
			end)
			wait()
		end
	end)
end
coroutine.wrap(ENLOZXY_fake_script)()
local function DJCP_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local RebirthEvent = ReplicatedStorage.Events.RebirthEvent -- RemoteEvent 
				RebirthEvent:FireServer(
					_G.Stage
				)
			end)
			wait()
		end
	end)
end
coroutine.wrap(DJCP_fake_script)()
local function TWVMDH_fake_script() -- TextBox.Controller 
	local script = Instance.new('LocalScript', TextBox)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.Focused:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.439216, 0.439216, 0.439216)}):Play()
		end)
	end)
	
	script.Parent.FocusLost:Connect(function()
		local sucess, err = pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			local textbox = script.Parent
			local text = textbox.Text
	
			local tonum = tonumber(text)
	
			if tonum ~= nil then
				_G.Stage = tonum
			else
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.698039, 0.0705882, 0.164706)}):Play()
				wait(1)
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			end
		end)
		
		if not sucess then print(err) end
	end)
end
coroutine.wrap(TWVMDH_fake_script)()
