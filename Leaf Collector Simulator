-- Scripts:

local function NBQPE_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService('ReplicatedStorage')
				local collectLeaf = ReplicatedStorage.Events.collectLeaf -- RemoteEvent
	
				collectLeaf:FireServer(_G.Stage)
			end)
			wait()
		end
	end)
end
coroutine.wrap(NBQPE_fake_script)()
local function PPRBUO_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local gemAreas = workspace:WaitForChild("GemAreas")
	
				for _, part in pairs(gemAreas:GetChildren()) do
					-- Проверяем, что это Part и у него есть дочерний MeshPart с именем "SampleGem"
					if part:IsA("BasePart") then
						local sampleGem = part:FindFirstChild("SampleGem")
						if sampleGem and sampleGem:IsA("MeshPart") then
							-- Находим все TouchInterest в sampleGem (обычно их может быть несколько)
							for _, touchInterest in pairs(sampleGem:GetChildren()) do
								if touchInterest:IsA("TouchTransmitter") then
									-- Здесь touchInterest — это TouchTransmitter, а не TouchInterest напрямую.
									-- Fire touch event: используем функцию firetouchinterest
									-- Fire touch began and ended events with a dummy part (например, локальный скрипт или свой Part)
	
									-- Для работы firetouchinterest нужно указать два объекта BasePart: один, который "касается", другой — который "принимает касание"
									-- Обычно вызывают так: firetouchinterest(part1, part2, 0) и firetouchinterest(part1, part2, 1)
									-- В данном случае чтобы активировать TouchInterest у sampleGem, нужен объект, который "касается" sampleGem.
	
									-- Для примера создадим временный Part (или можно использовать какой-то игровой объект, который есть в игре)
									-- Вот пример с использованием скрипта, который активирует касание sampleGem с игроком (если скрипт внутри LocalScript)
									-- Но если у тебя нет объекта, который должен касаться, то придется его создать.
	
									-- Пример с текущим игроком (если это LocalScript):
									local player = game.Players.LocalPlayer
									if player and player.Character then
										local character = player.Character
										for _, charPart in pairs(character:GetChildren()) do
											if charPart:IsA("BasePart") then
												firetouchinterest(charPart, sampleGem, 0)
												firetouchinterest(charPart, sampleGem, 1)
												break
											end
										end
									end
								end
							end
						end
					end
				end
			end)
			wait()
		end
	end)
end
coroutine.wrap(PPRBUO_fake_script)()
local function SZITO_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local ReplicatedStorage = game:GetService("ReplicatedStorage")
				local Players = game:GetService("Players")
	
				local RemoteFunction = ReplicatedStorage:WaitForChild("uxpRS")
					:WaitForChild("PetSystem")
					:WaitForChild("GlobalEvents")
					:WaitForChild("RemoteFunction")
	
				local petPlatformsFolder = workspace:WaitForChild("PetPlatforms")
	
				local player = Players.LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
				local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	
				local OPEN_DISTANCE = 10 -- расстояние для открытия
	
				for i = 1, 5 do
					local eggName = "Regular"..i
					local eggModel = petPlatformsFolder:FindFirstChild(eggName)
					if eggModel and eggModel:IsA("Model") then
						local part = eggModel.PrimaryPart or eggModel:FindFirstChildWhichIsA("BasePart")
						if part then
							local dist = (humanoidRootPart.Position - part.Position).Magnitude
							print("Distance to", eggName, dist)
							if dist <= OPEN_DISTANCE then
								print("Opening egg nearby:", eggName)
								RemoteFunction:InvokeServer(
									"PetOpenEvent",
									eggName,
									1,
									{}
								)
							end
						else
							warn("No suitable part found in model:", eggName)
						end
					else
						warn(eggName, "not found or not a model")
					end
				end
			end)
			wait()
		end
	end)
end
coroutine.wrap(SZITO_fake_script)()
local function QJDZUGU_fake_script() -- TextBox.Controller 
	local script = Instance.new('LocalScript', TextBox)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.Focused:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.439216, 0.439216, 0.439216)}):Play()
		end)
	end)
	
	script.Parent.FocusLost:Connect(function()
		local sucess, err = pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			local textbox = script.Parent
			local text = textbox.Text
	
			local tonum = tonumber(text)
	
			if tonum ~= nil then
				_G.Stage = tonum
			else
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.698039, 0.0705882, 0.164706)}):Play()
				wait(1)
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			end
		end)
		
		if not sucess then print(err) end
	end)
end
