-- Scripts:

local function LWJYFYJ_fake_script() -- Trigger.Controller 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local player = game.Players.LocalPlayer  -- Получаем текущего игрока
				local tycoonWorkspace = workspace.TycoonWorkspace.Tycoons  -- Папка с тайкунами
	
				-- Функция для поиска тайкуна по имени игрока
				local function findMyTycoon()
					for _, tycoon in pairs(tycoonWorkspace:GetChildren()) do
						print("Проверка тайкуна: " .. tostring(tycoon.Name))  -- Отладочный вывод
	
						-- Проверяем, есть ли в тайкуне папка "Owner"
						local owner = tycoon:FindFirstChild("Owner")
						if owner then
							local ownerName = tostring(owner.Value):match("^%s*(.-)%s*$")  -- Обрезаем пробелы
							local playerName = tostring(player.Name):match("^%s*(.-)%s*$")  -- Обрезаем пробелы
	
							print("Owner найден! Проверяем имя владельца: " .. tostring(ownerName))  -- Отладочный вывод
	
							-- Проверяем, совпадает ли имя владельца с ником игрока
							if ownerName == playerName then
								print("Найден ваш тайкун: " .. tycoon.Name)  -- Выводим название тайкуна, если имя совпадает
	
								-- Проверка на наличие "PurchasedObjects" в тайкуне
								local purchasedObjects = tycoon:FindFirstChild("PurchasedObjects")
								if purchasedObjects then
									-- Проверяем модель Dropper1
									local dropper1 = purchasedObjects:FindFirstChild("Dropper1")
									if dropper1 then
										print("Найден Dropper1 в PurchasedObjects!")
	
										-- Проходим по всем моделям внутри Dropper1
										for _, model in pairs(dropper1:GetChildren()) do
											if model:IsA("Model") then  -- Проверяем, что это модель
												print("Проверка модели: " .. model.Name)  -- Отладочный вывод
	
												-- Ищем Clicker внутри каждой модели
												local clicker = model:FindFirstChild("Clicker")
												if clicker then
													-- Ищем ClickDetector в Clicker
													local clickDetector = clicker:FindFirstChild("ClickDetector")
													if clickDetector then
														print("Найден ClickDetector в модели " .. model.Name)
														-- Эмулируем клик на ClickDetector
														fireclickdetector(clickDetector)
														return tycoon, clickDetector  -- Возвращаем тайкун и его ClickDetector
													else
														print("ClickDetector не найден в Clicker в модели " .. model.Name)
													end
												else
													print("Clicker не найден в модели " .. model.Name)
												end
											end
										end
									else
										print("Dropper1 не найден в PurchasedObjects!")
									end
								else
									print("PurchasedObjects не найден в тайкуне!")
								end
							else
								print("Тайкун " .. tostring(tycoon.Name) .. " не ваш. Владельцем является: " .. ownerName)  -- Выводим для других тайкунов
							end
						end
					end
					return nil  -- Если тайкун не найден
				end
	
				-- Найти свой тайкун и ClickDetector
				local tycoon, clickDetector = findMyTycoon()
	
				if tycoon and clickDetector then
					print("Найден свой тайкун: " .. tostring(tycoon.Name))
					print("Найден ClickDetector в тайкуне!")
	
					-- Эмулируем клик на ClickDetector
					fireclickdetector(clickDetector)
				else
					print("Тайкун не найден или ошибка!")
				end
			end)
			wait()
		end
	end)
end
coroutine.wrap(LWJYFYJ_fake_script)()
local function FDBFR_fake_script() -- Trigger_2.Controller 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,24,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local player = game.Players.LocalPlayer  -- Получаем текущего игрока
				local tycoonWorkspace = workspace.TycoonWorkspace.Tycoons  -- Папка с тайкунами
	
				-- Функция для поиска тайкуна по имени игрока
				local function findMyTycoon()
					for _, tycoon in pairs(tycoonWorkspace:GetChildren()) do
						print("Проверка тайкуна: " .. tostring(tycoon.Name))  -- Отладочный вывод
	
						-- Проверяем, есть ли в тайкуне папка "Owner"
						local owner = tycoon:FindFirstChild("Owner")
						if owner then
							-- Преобразуем owner.Value в строку, чтобы избежать ошибки
							print("Owner найден! Проверяем имя владельца: " .. tostring(owner.Value))  -- Отладочный вывод
	
							-- Проверяем, совпадает ли имя владельца с ником игрока
							if tostring(owner.Value) == player.Name then
								print("Найден ваш тайкун: " .. tostring(tycoon.Name))  -- Выводим название тайкуна
								-- Если да, то возвращаем найденный тайкун и его Giver
								local giver = tycoon:FindFirstChild("Essentials"):FindFirstChild("Giver")
								return giver
							else
								print("Тайкун " .. tostring(tycoon.Name) .. " не ваш. Владельцем является: " .. tostring(owner.Value))  -- Выводим для других тайкунов
							end
						end
					end
					return nil  -- Если тайкун не найден
				end
	
				-- Найти свой тайкун и Giver
				local giver = findMyTycoon()
	
				if giver then
					-- Проверяем наличие TouchInterest внутри Giver
					local touchInterest = giver:FindFirstChild("TouchInterest")
					if touchInterest then
						-- Симулируем срабатывание TouchInterest
						firetouchinterest(giver, game.Players.LocalPlayer.Character.HumanoidRootPart, 0)  -- Контакт (нажатие)
						task.wait()  -- Небольшая задержка
						firetouchinterest(giver, game.Players.LocalPlayer.Character.HumanoidRootPart, 1)  -- Отпускание
						print("TouchInterest был активирован дистанционно!")
					else
						warn("TouchInterest не найден в объекте Giver!")
					end
				else
					warn("Тайкун не найден!")
				end
			end)
			wait()
		end
	end)
end
coroutine.wrap(FDBFR_fake_script)()
local function GLXQJ_fake_script() -- Trigger_3.Controller 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x = TweenInfo.new(0.7)
	local status = false
	
	-- Получаем игрока и его персонажа
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	
	-- Функция для включения и выключения Noclip
	local Clipon = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			-- Включаем Noclip
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0, 24, 0, 0)}):Play()
			status = true
			Clipon = true
			-- Симулируем Noclip (выключаем CanCollide для всех BasePart персонажа)
			game:GetService("RunService").Stepped:Connect(function()
				if Clipon then
					for _, part in pairs(character:GetChildren()) do
						if part:IsA("BasePart") then
							part.CanCollide = false  -- Отключаем столкновение для всех частей тела
						end
					end
				else
					for _, part in pairs(character:GetChildren()) do
						if part:IsA("BasePart") then
							part.CanCollide = true  -- Включаем столкновение для всех частей тела
						end
					end
				end
			end)
		else
			-- Выключаем Noclip
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0, 0, 0, 0)}):Play()
			status = false
			Clipon = false
		end
	end)
	
end
coroutine.wrap(GLXQJ_fake_script)()
local function VKCH_fake_script() -- Trigger_4.Controller 
	local script = Instance.new('LocalScript', Trigger_4)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x = TweenInfo.new(0.7)
	local status = false
	local defaultWalkSpeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed  -- Стандартная скорость
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			-- Включаем увеличение скорости
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0, 24, 0, 0)}):Play()
	
			-- Увеличиваем скорость до 50
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
			status = true
	
			-- Цикл, который поддерживает скорость 50
			spawn(function()
				while status do
					wait(0.1)  -- Проверяем каждые 0.1 секунды
					if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= 50 then
						game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
					end
				end
			end)
	
		else
			-- Выключаем увеличение скорости
			ts:Create(script.Parent.Parent.In, info, {BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame, x, {Position = UDim2.new(0, 0, 0, 0)}):Play()
	
			-- Возвращаем стандартную скорость
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = defaultWalkSpeed
			status = false
		end
	end)
	
end
coroutine.wrap(VKCH_fake_script)()
local function UANJZ_fake_script() -- Trigger_5.Controller 
	local script = Instance.new('LocalScript', Trigger_5)

	local TweenService = game:GetService("TweenService")
	local player = game.Players.LocalPlayer
	
	script.Parent.MouseEnter:Connect(function()
		TweenService:Create(script.Parent.Parent, TweenInfo.new(1), {BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		TweenService:Create(script.Parent.Parent, TweenInfo.new(1), {BackgroundTransparency = 0.8}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local success, err = pcall(function()
			-- Находим всех игроков в игре
			local players = game.Players:GetPlayers()
			local randomPlayer = players[math.random(1, #players)]  -- Выбираем случайного игрока
	
			-- Убедимся, что случайный игрок существует
			if randomPlayer and randomPlayer.Character then
				-- Получаем позицию случайного игрока
				local targetPosition = randomPlayer.Character.HumanoidRootPart.Position
				-- Телепортируем персонажа в эту позицию
				player.Character:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			end
		end)
	
		-- Обработка ошибок
		if not success then
			-- Если произошла ошибка, меняем цвет кнопки в красный
			TweenService:Create(script.Parent.Parent, TweenInfo.new(2), {BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait(2)
			TweenService:Create(script.Parent.Parent, TweenInfo.new(2), {BackgroundColor3 = Color3.new(1, 1, 1)}):Play()
		else
			-- Если все прошло успешно, меняем цвет кнопки в зеленый
			TweenService:Create(script.Parent.Parent, TweenInfo.new(2), {BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait(2)
			TweenService:Create(script.Parent.Parent, TweenInfo.new(2), {BackgroundColor3 = Color3.new(1, 1, 1)}):Play()
		end
	end)
	
end
coroutine.wrap(UANJZ_fake_script)()
